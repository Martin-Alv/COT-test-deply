  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tipo_Movimiento"."Tipo_Movimiento_Id" "Tipo_Movimiento_Id"
	,"Tipo_Movimiento"."Tipo_Movimiento" "Tipo_Movimiento"
	,"Tipo_Movimiento"."Es_Solicitud_Devolucion" "Es_Solicitud_Devolucion"
	,"Tipo_Movimiento"."Es_Solicitud_Transferencia" "Es_Solicitud_Transferencia"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Tipo_Movimiento"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Tipo_Movimiento"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Movimiento"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Tipo_Movimiento" "Tipo_Movimiento"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Movimiento"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Movimiento"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Movimiento"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
