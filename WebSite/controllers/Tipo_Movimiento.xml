  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 

SET @Already_Exists = 0 

SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Movimiento 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Movimiento) )=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Movimiento) ) 

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](256,1,73,@Tipo_Movimiento) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](256,1,73,@Tipo_Movimiento) 
   RETURN 
END]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE  
   APP.Tipo_Movimiento 
SET 
    Creado_Por_Id = @BusinessRules_UserId 
   ,Fecha_Creacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL) 
   ,Actualizado_Por_Id = @BusinessRules_UserId 
   ,Fecha_Ultima_Actualizacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL) 
WHERE 
   Tipo_Movimiento_Id = @Tipo_Movimiento_Id ]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT 
    TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Movimiento 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Movimiento))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Movimiento)) 
AND Tipo_Movimiento_Id <> @Tipo_Movimiento_Id 
 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](257,1,73,@Tipo_Movimiento) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](257,1,73,@Tipo_Movimiento) 
   RETURN 
END]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="After"><![CDATA[UPDATE  
   APP.Tipo_Movimiento 
SET 
