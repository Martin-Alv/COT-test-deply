  <businessRules>
    <rule id="r101" commandName="Insert|Update" type="Sql" phase="Before"><![CDATA[DECLARE @Cantidad_Digitos_Min INT = 0;
DECLARE @Cantidad_Digitos_Max INT = 0;
DECLARE @Cantidad_Digitos INT = 0;
DECLARE @Cantidad_Digitos_Valido BIT;

SET @Cantidad_Digitos_Min = ISNULL((SELECT TOP 1 Valor_Cuantitativo FROM dbo.Configuracion_Sistema WHERE Configuracion_Sistema_Id = 14),6);
SET @Cantidad_Digitos_Max = ISNULL((SELECT TOP 1 Valor_Cuantitativo FROM dbo.Configuracion_Sistema WHERE Configuracion_Sistema_Id = 15),9);
SET @Cantidad_Digitos = LEN(@Nip);

IF (@Cantidad_Digitos >= @Cantidad_Digitos_Min AND @Cantidad_Digitos <= @Cantidad_Digitos_Max)
BEGIN
	SET @Cantidad_Digitos_Valido = 1;	
END
ELSE
BEGIN
	SET @Cantidad_Digitos_Valido = 0;	
END

IF @Cantidad_Digitos_Valido = 0
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](142,1,43,'Cantidad válido: Mínimo '  + CONVERT(VARCHAR(10),@Cantidad_Digitos_Min) + ', Máximo ' + CONVERT(VARCHAR(10),@Cantidad_Digitos_Max))
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](142,1,43,'Cantidad válido: Mínimo '  + CONVERT(VARCHAR(10),@Cantidad_Digitos_Min) + ', Máximo ' + CONVERT(VARCHAR(10),@Cantidad_Digitos_Max))
   RETURN
END 
]]></rule>
    <rule id="r102" commandName="Insert|Update" type="Code" phase="After" />
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Usuario_Aplicacion_Acceso
WHERE
	Usuario_Aplicacion_Id = @Usuario_Aplicacion_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](148,1,44,@Nombre_Completo)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Usuario_Aplicacion_Acceso
WHERE
	Usuario_Aplicacion_Id = @Usuario_Aplicacion_Id
	AND Usuario_Aplicacion_Acceso_Id <> @Usuario_Aplicacion_Acceso_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](149,1,44,@Nombre_Completo)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
