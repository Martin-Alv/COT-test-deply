FROM
   APP.Etiqueta_Proveedor
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Etiqueta_Proveedor_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Etiqueta_Proveedor_Id))
