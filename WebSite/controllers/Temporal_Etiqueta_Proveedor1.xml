    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT
SET @Already_Exists = 0
SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Temporal_Etiqueta_Proveedor
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Temporal_Etiqueta_Proveedor_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Temporal_Etiqueta_Proveedor_Id))
IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1325,1,10,@Temporal_Etiqueta_Proveedor_Id)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1325,1,10,@Temporal_Etiqueta_Proveedor_Id)
   RETURN
END
-----------------------------------------------------


]]></rule>
