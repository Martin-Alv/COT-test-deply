    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Orden_Produccion"."Orden_Produccion_Id" "Orden_Produccion_Id"
	,"Orden_Produccion"."Numero_Orden_Produccion" "Numero_Orden_Produccion"
	,"Orden_Produccion"."Fecha_Produccion" "Fecha_Produccion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Orden_Produccion"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Orden_Produccion"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Orden_Produccion"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Orden_Produccion"."Cantidad" "Cantidad"
	,"Orden_Produccion"."Estatus_Orden_Produccion_Id" "Estatus_Orden_Produccion_Id"
	,"Estatus_Orden_Produccion"."Estatus_Orden_Produccion" "Estatus_Orden_Produccion"
from "dbo"."Orden_Produccion" "Orden_Produccion"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Orden_Produccion"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Orden_Produccion"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Orden_Produccion"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Estatus_Orden_Produccion" "Estatus_Orden_Produccion" on "Orden_Produccion"."Estatus_Orden_Produccion_Id" = "Estatus_Orden_Produccion"."Estatus_Orden_Produccion_Id"
]]></text>
    </command>
