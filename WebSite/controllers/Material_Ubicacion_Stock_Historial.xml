	,"Material_Ubicacion_Stock_Historial"."Movimiento_Ruta_Optima_Id" "Movimiento_Ruta_Optima_Id"
	,"Material_Ubicacion_Stock_Historial"."PO" "PO"
from "APP"."Material_Ubicacion_Stock_Historial" "Material_Ubicacion_Stock_Historial"
	left join "APP"."Material" "Material_" on "Material_Ubicacion_Stock_Historial"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Tipo_Almacenamiento" "Tipo_Almacenamiento_" on "Material_Ubicacion_Stock_Historial"."Tipo_Almacenamiento_Id" = "Tipo_Almacenamiento_"."Tipo_Almacenamiento_Id"
