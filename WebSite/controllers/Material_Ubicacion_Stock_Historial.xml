	,"Tipo_Almacenamiento_"."Tipo_Almacenamiento" "Tipo_Almacenamiento"
	,"Ubicacion_Almacenamiento_"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	,"Material_Ubicacion_Stock_Historial"."Tipo_Ubicacion_Id" "Tipo_Ubicacion_Id"
	,"Tipo_Ubicacion_"."Clave_Tipo_Ubicacion" "Clave_Tipo_Ubicacion"
