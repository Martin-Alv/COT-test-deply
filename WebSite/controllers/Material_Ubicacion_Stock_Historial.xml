  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT
SET @Already_Exists = 0
SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Material_Ubicacion_Stock_Historial
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Material_Ubicacion_Stock_Historial_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Material_Ubicacion_Stock_Historial_Id))
IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1397,1,10,@Material_Ubicacion_Stock_Historial_Id)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1397,1,10,@Material_Ubicacion_Stock_Historial_Id)
   RETURN
END
]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE 
   APP.Material_Ubicacion_Stock_Historial
SET
    Creado_Por_Id = @BusinessRules_UserId
   ,Fecha_Creacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
   ,Actualizado_Por_Id = @BusinessRules_UserId
   ,Fecha_Ultima_Actualizacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
WHERE
   Material_Ubicacion_Stock_Historial_Id = @Material_Ubicacion_Stock_Historial_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT
SET @Already_Exists = 0
SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Material_Ubicacion_Stock_Historial
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Material_Ubicacion_Stock_Historial_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Material_Ubicacion_Stock_Historial_Id))
AND Material_Ubicacion_Stock_Historial_Id <> @Material_Ubicacion_Stock_Historial_Id

IF @Already_Exists = 1
BEGIN
