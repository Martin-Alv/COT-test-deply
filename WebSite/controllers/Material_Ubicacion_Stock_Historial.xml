	,"Material_Ubicacion_Stock_Historial"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Material_Ubicacion_Stock_Historial"."Estatus_Registro_Id" "Estatus_Registro_Id"
