	left join "APP"."Tipo_Ubicacion" "Tipo_Ubicacion_" on "Material_Ubicacion_Stock_Historial"."Tipo_Ubicacion_Id" = "Tipo_Ubicacion_"."Tipo_Ubicacion_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde_" on "Material_Ubicacion_Stock_Historial"."Registrado_Desde_Id" = "Registrado_Desde_"."Registrado_Desde_Id"
	left join "APP"."Ruta_Optima" "Ruta_Optima_" on "Material_Ubicacion_Stock_Historial"."Ruta_Optima_Id" = "Ruta_Optima_"."Ruta_Optima_Id"
	left join "APP"."Material_Surtido" "Material_Surtido_" on "Material_Ubicacion_Stock_Historial"."Material_Surtido_Id" = "Material_Surtido_"."Material_Surtido_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material_Ubicacion_Stock_Historial"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material_Ubicacion_Stock_Historial"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material_Ubicacion_Stock_Historial"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
