	,"Ruta_Optima_"."Transaccion_Id" "Ruta_Optima_Transaccion_Id"
	,"Material_Ubicacion_Stock_Historial"."Material_Surtido_Id" "Material_Surtido_Id"
	,"Material_Surtido_"."Transaccion_Id" "Material_Surtido_TransaccionId"
