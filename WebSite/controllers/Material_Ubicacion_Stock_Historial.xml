	,"Material_Ubicacion_Stock_Historial"."Total_Stock" "Total_Stock"
	,"Material_Ubicacion_Stock_Historial"."Til" "Til"
	,"Material_Ubicacion_Stock_Historial"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Registrado_Desde_"."Registrado_Desde" "Registrado_Desde"
