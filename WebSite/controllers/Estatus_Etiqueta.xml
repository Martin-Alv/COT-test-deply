	,"Estatus_Etiqueta"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Estatus_Etiqueta" "Estatus_Etiqueta"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Estatus_Etiqueta"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Estatus_Etiqueta"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Estatus_Etiqueta"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
