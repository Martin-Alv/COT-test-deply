  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Ubicacion_Almacenamiento] 
WHERE 
    dbo.fn_Sustituir_Caracteres_Especiales(Lower(Ubicacion_Almacenamiento))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Ubicacion_Almacenamiento)) 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](248,1,69,@Ubicacion_Almacenamiento) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](248,1,69,@Ubicacion_Almacenamiento) 
   RETURN 
END ]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT    TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Ubicacion_Almacenamiento]
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Ubicacion_Almacenamiento))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Ubicacion_Almacenamiento)) 
   AND Ubicacion_Almacenamiento_Id <> @Ubicacion_Almacenamiento_Id	

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](249,1,69,@Ubicacion_Almacenamiento) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](249,1,69,@Ubicacion_Almacenamiento) 
   RETURN 
END ]]></rule>
