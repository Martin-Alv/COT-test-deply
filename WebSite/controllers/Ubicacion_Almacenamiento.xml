    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Ubicacion_Almacenamiento"."Ubicacion_Almacenamiento_Id" "Ubicacion_Almacenamiento_Id"
	,"Ubicacion_Almacenamiento"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Ubicacion_Almacenamiento"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Ubicacion_Almacenamiento"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Ubicacion_Almacenamiento"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Tipo_Ubicacion"."Clave_Tipo_Ubicacion" "Clave_Tipo_Ubicacion"
	,"Ubicacion_Almacenamiento"."Tipo_Ubicacion_Id" "Tipo_Ubicacion_Id"
	,"Ubicacion_Almacenamiento"."Aplica_Para_Ruta_Optima" "Aplica_Para_Ruta_Optima"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Ubicacion_Almacenamiento"."Registrado_Desde_Id" "Registrado_Desde_Id"
from "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Ubicacion_Almacenamiento"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Ubicacion_Almacenamiento"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Ubicacion_Almacenamiento"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Tipo_Ubicacion" "Tipo_Ubicacion" on "Ubicacion_Almacenamiento"."Tipo_Ubicacion_Id" = "Tipo_Ubicacion"."Tipo_Ubicacion_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Ubicacion_Almacenamiento"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
]]></text>
    </command>
