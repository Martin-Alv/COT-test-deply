	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Estatus_Movimiento" "Estatus_Movimiento"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Estatus_Movimiento"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
