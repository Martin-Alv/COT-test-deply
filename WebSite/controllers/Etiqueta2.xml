    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Etiqueta"."Etiqueta_Id" "Etiqueta_Id"
	,"Etiqueta"."Numero_Etiqueta" "Numero_Etiqueta"
	,"Etiqueta"."Bitacora_Captura_Sap_Id" "Bitacora_Captura_Sap_Id"
	,"Bitacora_Captura_Sap"."Bitacora_Captura_Sap_Id" "Bitacora_Captura_Sap"
	,"Etiqueta"."Captura_Id" "Captura_Id"
	,"Etiqueta"."Material" "Material"
	,"Etiqueta"."Cantidad" "Cantidad"
	,"Etiqueta"."Ubicacion_Almacenamiento_Origen_Id" "Ubicacion_Almacenamiento_Origen_Id"
	,"Ubicacion_Almacenamiento_Origen"."Ubicacion_Almacenamiento" "Origen"
	,"Etiqueta"."Ubicacion_Almacenamiento_Origen" "Ubicacion_Almacenamiento_Origen"
	,"Etiqueta"."Ubicacion_Almacenamiento_Destino_Id" "Ubicacion_Almacenamiento_Destino_Id"
	,"Ubicacion_Almacenamiento_Destino"."Ubicacion_Almacenamiento" "Destino"
	,"Etiqueta"."Ubicacion_Almacenamiento_Destino" "Ubicacion_Almacenamiento_Destino"
	,"Etiqueta"."Html_QR" "Html_QR"
	,"Etiqueta"."Codigo_De_Barra" "Codigo_De_Barra"
	,"Etiqueta"."Estatus_Etiqueta_Id" "Estatus_Etiqueta_Id"
	,"Estatus_Etiqueta"."Estatus_Etiqueta" "Estatus_Etiqueta"
	,"Impreso_Por"."Usuario_Sistema" "Impreso_Por"
	,"Etiqueta"."Fecha_Impresion" "Fecha_Impresion"
	,"Etiqueta"."Equipo_Impresion" "Equipo_Impresion"
	,"Etiqueta"."Error" "Error"
	,"Etiqueta"."Mensaje_Sistema_Id" "Mensaje_Sistema_Id"
	,"Mensaje_Sistema_"."Mensaje_Sistema" "Mensaje_Sistema"
	,"Usuario_Surtidor"."Usuario_Sistema" "Usuario_Surtidor"
	,"Etiqueta"."Transaccion_Captura" "Transaccion_Captura"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Etiqueta"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Etiqueta"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Etiqueta"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Etiqueta"."Material_Id" "Material_Id"
	,"Material"."Material" "Material_Material"
	,"Etiqueta"."Usuario_Surtidor" "UsuarioSurtidor"
	,"Etiqueta"."Orden_Produccion_Id" "Orden_Produccion_Id"
	,"Orden_Produccion"."Numero_Orden_Produccion" "Numero_Orden_Produccion"
	,"Etiqueta"."Tipo_Material_Id" "Tipo_Material_Id"
	,"Tipo_Material"."Tipo_Material" "Tipo_Material"
	,"Etiqueta"."Clase_Material_Id" "Clase_Material_Id"
	,"Clase_Material"."Clase_Material" "Clase_Material"
	,"Etiqueta"."Material_Empaque_Id" "Material_Empaque_Id"
	,"Material_Empaque"."Material_Empaque" "Material_Empaque"
	,"Etiqueta"."Solicitud_Id" "SolicitudId"
from "APP"."Etiqueta" "Etiqueta"
	left join "APP"."Estatus_Etiqueta" "Estatus_Etiqueta" on "Etiqueta"."Estatus_Etiqueta_Id" = "Estatus_Etiqueta"."Estatus_Etiqueta_Id"
	left join "dbo"."Mensaje_Sistema" "Mensaje_Sistema_" on "Etiqueta"."Mensaje_Sistema_Id" = "Mensaje_Sistema_"."Mensaje_Sistema_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Etiqueta"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Etiqueta"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Etiqueta"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Impreso_Por" on "Etiqueta"."Impreso_Por" = "Impreso_Por"."Usuario_Sistema_Id"
 and "Etiqueta"."Usuario_Surtidor" = "Impreso_Por"."Aplicacion_Id"
	left join "dbo"."Usuario_V" "Usuario_Surtidor" on "Etiqueta"."Usuario_Surtidor" = "Usuario_Surtidor"."Usuario_Sistema_Id"
	left join "APP"."Material" "Material" on "Etiqueta"."Material_Id" = "Material"."Material_Id"
	left join "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento_Origen" on "Etiqueta"."Ubicacion_Almacenamiento_Origen_Id" = "Ubicacion_Almacenamiento_Origen"."Ubicacion_Almacenamiento_Id"
	left join "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento_Destino" on "Etiqueta"."Ubicacion_Almacenamiento_Destino_Id" = "Ubicacion_Almacenamiento_Destino"."Ubicacion_Almacenamiento_Id"
	left join "APP"."Bitacora_Captura_Sap" "Bitacora_Captura_Sap" on "Etiqueta"."Bitacora_Captura_Sap_Id" = "Bitacora_Captura_Sap"."Bitacora_Captura_Sap_Id"
	left join "dbo"."Orden_Produccion" "Orden_Produccion" on "Etiqueta"."Orden_Produccion_Id" = "Orden_Produccion"."Orden_Produccion_Id"
	left join "APP"."Tipo_Material" "Tipo_Material" on "Etiqueta"."Tipo_Material_Id" = "Tipo_Material"."Tipo_Material_Id"
	left join "APP"."Clase_Material" "Clase_Material" on "Etiqueta"."Clase_Material_Id" = "Clase_Material"."Clase_Material_Id"
	left join "APP"."Material_Empaque" "Material_Empaque" on "Etiqueta"."Material_Empaque_Id" = "Material_Empaque"."Material_Empaque_Id"
]]></text>
    </command>
