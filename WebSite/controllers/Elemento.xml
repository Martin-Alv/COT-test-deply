<?xml version="1.0" encoding="utf-8"?>
<dataController name="Elemento" conflictDetection="overwriteChanges" label="Elemento" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Elemento"."Elemento_Id" "Elemento_Id"
	,"Elemento"."Elemento_Descripcion" "Elemento_Descripcion"
	,"Elemento"."Transaccion_Id" "Transaccion_Id"
	,"Elemento"."Creado_Por_Id" "Creado_Por_Id"
	,"Elemento"."Fecha_Creacion" "Fecha_Creacion"
	,"Elemento"."Actualizado_Por_Id" "Actualizado_Por_Id"
	,"Elemento"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Elemento"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
from "APP"."Elemento" "Elemento"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Elemento"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
]]></text>
    </command>
    <command id="Elemento_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Elemento_Id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Elemento_Id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Elemento Id" readOnly="true" />
    <field name="Elemento_Descripcion" type="String" allowNulls="false" label="Elemento Descripcion" length="250" showInSummary="true" />
    <field name="Transaccion_Id" type="String" allowNulls="false" default="(newid())" label="Transaccion Id" length="50" showInSummary="true" />
    <field name="Creado_Por_Id" type="Guid" allowNulls="false" default="(CONVERT([uniqueidentifier],'5C7568FD-6412-474E-A099-006FAD27CD02',(0)))" label="Creado Por Id" showInSummary="true" />
    <field name="Fecha_Creacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Creacion" showInSummary="true" />
    <field name="Actualizado_Por_Id" type="Guid" allowNulls="false" default="(CONVERT([uniqueidentifier],'5C7568FD-6412-474E-A099-006FAD27CD02',(0)))" label="Actualizado Por Id" showInSummary="true" />
    <field name="Fecha_Ultima_Actualizacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Ultima Actualizacion" />
    <field name="Estatus_Registro_Id" type="Int32" allowNulls="false" default="((1))" label="Estatus Registro Id">
      <items style="Lookup" dataController="Estatus_Registro" newDataView="createForm1" searchOnStart="true" dataValueField="Estatus_Registro_Id" dataTextField="Estatus_Registro" />
    </field>
    <field name="Estatus_Registro_Estatus_Registro" type="String" readOnly="true" label="Estatus Registro" length="150" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Elemento">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="Elemento_Descripcion" />
        <dataField fieldName="Transaccion_Id" columns="50" />
        <dataField fieldName="Creado_Por_Id" columns="15" />
        <dataField fieldName="Fecha_Creacion" columns="20" />
        <dataField fieldName="Actualizado_Por_Id" columns="15" />
        <dataField fieldName="Fecha_Ultima_Actualizacion" columns="20" />
        <dataField fieldName="Estatus_Registro_Id" aliasFieldName="Estatus_Registro_Estatus_Registro" hyperlinkFormatString="~/pages/Estatus_Registro?Estatus_Registro_Id={Estatus_Registro_Id}&amp;_controller=Estatus_Registro&amp;_command=Select&amp;_argument=editForm1" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review APP Elemento">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
