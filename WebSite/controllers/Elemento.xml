          <dataFields>
            <dataField fieldName="Elemento_Descripcion" />
            <dataField fieldName="Transaccion_Id" columns="50" />
            <dataField fieldName="Creado_Por_Id" columns="15" />
            <dataField fieldName="Fecha_Creacion" columns="20" />
            <dataField fieldName="Actualizado_Por_Id" columns="15" />
            <dataField fieldName="Fecha_Ultima_Actualizacion" columns="20" />
            <dataField fieldName="Estatus_Registro_Id" aliasFieldName="Estatus_Registro_Estatus_Registro" hyperlinkFormatString="~/pages/Estatus_Registro?Estatus_Registro_Id={Estatus_Registro_Id}&amp;_controller=Estatus_Registro&amp;_command=Select&amp;_argument=editForm1" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New APP Elemento">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="Elemento_Descripcion" />
            <dataField fieldName="Transaccion_Id" columns="50" />
            <dataField fieldName="Creado_Por_Id" columns="15" />
            <dataField fieldName="Fecha_Creacion" columns="20" />
            <dataField fieldName="Actualizado_Por_Id" columns="15" />
            <dataField fieldName="Fecha_Ultima_Actualizacion" columns="20" />
            <dataField fieldName="Estatus_Registro_Id" aliasFieldName="Estatus_Registro_Estatus_Registro" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
