<?xml version="1.0" encoding="utf-8"?>
<dataController name="Solicitud_Documentacion" conflictDetection="overwriteChanges" label="Solicitud Documentacion" handler="eaton.Rules.Solicitud_DocumentacionBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Solicitud_Documentacion"."Solicitud_Documentacion_Id" "Solicitud_Documentacion_Id"
	,"Solicitud_Documentacion"."Solicitud_Id" "Solicitud_Id"
	,"Solicitud_Documentacion"."Comentarios" "Comentarios"
	,"Solicitud_Documentacion"."Picture" "Picture"
	,"Solicitud_Documentacion"."PictureFileName" "PictureFileName"
	,"Solicitud_Documentacion"."PictureContentType" "PictureContentType"
	,"Solicitud_Documentacion"."PictureLength" "PictureLength"
	,"Solicitud_Documentacion"."Es_Comentario" "Es_Comentario"
	,"Solicitud_Documentacion"."Tipo_Comentario_Id" "Tipo_Comentario_Id"
	,"Tipo_Comentario"."Tipo_Comentario" "Tipo_Comentario"
	,"Solicitud_Documentacion"."Es_Imagen" "Es_Imagen"
	,"Solicitud_Documentacion"."Tipo_Archivo_Id" "Tipo_Archivo_Id"
	,"Tipo_Archivo_"."Tipo_Archivo" "Tipo_Archivo"
	,"Solicitud_Documentacion"."Es_Motivo_Cancelacion" "Es_Motivo_Cancelacion"
	,"Solicitud_Documentacion"."Motivo_Cancelacion_Id" "Motivo_Cancelacion_Id"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Solicitud_Documentacion"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Solicitud_Documentacion"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Solicitud_Documentacion"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Solicitud_Documentacion" "Solicitud_Documentacion"
	left join "APP"."Tipo_Archivo" "Tipo_Archivo_" on "Solicitud_Documentacion"."Tipo_Archivo_Id" = "Tipo_Archivo_"."Tipo_Archivo_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Solicitud_Documentacion"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Solicitud_Documentacion"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Solicitud_Documentacion"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Tipo_Comentario" "Tipo_Comentario" on "Solicitud_Documentacion"."Tipo_Comentario_Id" = "Tipo_Comentario"."Tipo_Comentario_Id"
]]></text>
    </command>
    <command id="Solicitud_Documentacion_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Solicitud_Documentacion_Id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Solicitud_Documentacion_Id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Solicitud Documentacion Id" readOnly="true" />
