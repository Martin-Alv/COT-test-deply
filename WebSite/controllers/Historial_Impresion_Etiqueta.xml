  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Historial_Impresion_Etiqueta"."Historial_Impresion_Etiqueta_Id" "Historial_Impresion_Etiqueta_Id"
	,"Historial_Impresion_Etiqueta"."Etiqueta_Proveedor_Id" "Etiqueta_Proveedor_Id"
	,"Etiqueta_Proveedor_"."Folio" "Etiqueta_Proveedor"
	,"Historial_Impresion_Etiqueta"."Etiqueta_Detalle_Proveedor_Id" "Etiqueta_Detalle_Proveedor_Id"
	,"Etiqueta_Detalle_Proveedor_"."Folio" "Etiqueta_Detalle_Proveedor"
	,"Historial_Impresion_Etiqueta"."Tipo_Etiqueta_Id" "Tipo_Etiqueta_Id"
	,"Tipo_Etiqueta_"."Tipo_Etiqueta" "Tipo_Etiqueta"
	,"Historial_Impresion_Etiqueta"."Tamanio_Etiqueta_Id" "Tamanio_Etiqueta_Id"
	,"Tamanio_Etiqueta_"."Tamanio_Etiqueta" "Tamanio_Etiqueta"
	,"Historial_Impresion_Etiqueta"."Fecha_Generacion" "Fecha_Generacion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Historial_Impresion_Etiqueta"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Historial_Impresion_Etiqueta"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Historial_Impresion_Etiqueta"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Historial_Impresion_Etiqueta" "Historial_Impresion_Etiqueta"
	left join "APP"."Etiqueta_Proveedor" "Etiqueta_Proveedor_" on "Historial_Impresion_Etiqueta"."Etiqueta_Proveedor_Id" = "Etiqueta_Proveedor_"."Etiqueta_Proveedor_Id"
	left join "APP"."Etiqueta_Detalle_Proveedor" "Etiqueta_Detalle_Proveedor_" on "Historial_Impresion_Etiqueta"."Etiqueta_Detalle_Proveedor_Id" = "Etiqueta_Detalle_Proveedor_"."Etiqueta_Detalle_Proveedor_Id"
	left join "APP"."Tipo_Etiqueta" "Tipo_Etiqueta_" on "Historial_Impresion_Etiqueta"."Tipo_Etiqueta_Id" = "Tipo_Etiqueta_"."Tipo_Etiqueta_Id"
	left join "APP"."Tamanio_Etiqueta" "Tamanio_Etiqueta_" on "Historial_Impresion_Etiqueta"."Tamanio_Etiqueta_Id" = "Tamanio_Etiqueta_"."Tamanio_Etiqueta_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Historial_Impresion_Etiqueta"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Historial_Impresion_Etiqueta"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Historial_Impresion_Etiqueta"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
