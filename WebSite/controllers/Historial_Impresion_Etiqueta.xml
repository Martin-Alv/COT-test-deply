  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT
SET @Already_Exists = 0
SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Historial_Impresion_Etiqueta
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Historial_Impresion_Etiqueta_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Historial_Impresion_Etiqueta_Id))
IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1316,1,10,@Historial_Impresion_Etiqueta_Id)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1316,1,10,@Historial_Impresion_Etiqueta_Id
   RETURN
END
]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE 
