    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Material_Inventario_Teorico"."Material_Inventario_Teorico_Id" "Material_Inventario_Teorico_Id"
	,"Material_Inventario_Teorico"."Material_Id" "Material_Id"
	,"Material_"."Material" "Material_Material"
	,"Material_Inventario_Teorico"."Tipo_Ubicacion_Id" "Tipo_Ubicacion_Id"
	,"Tipo_Ubicacion_"."Clave_Tipo_Ubicacion" "Clave_Tipo_Ubicacion"
	,"Material_Inventario_Teorico"."Stock_Teorico" "Stock_Teorico"
	,"Material_Inventario_Teorico"."Fecha_Inicio" "Fecha_Inicio"
	,"Material_Inventario_Teorico"."Fecha_Fin" "Fecha_Fin"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Material_Inventario_Teorico"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Material_Inventario_Teorico"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Material_Inventario_Teorico"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Material_Inventario_Teorico" "Material_Inventario_Teorico"
	left join "APP"."Material" "Material_" on "Material_Inventario_Teorico"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Tipo_Ubicacion" "Tipo_Ubicacion_" on "Material_Inventario_Teorico"."Tipo_Ubicacion_Id" = "Tipo_Ubicacion_"."Tipo_Ubicacion_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material_Inventario_Teorico"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material_Inventario_Teorico"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material_Inventario_Teorico"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
