  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Material_Inventario_Teorico]
WHERE 
  [Material_Id] = @Material_Id
   AND [Tipo_Ubicacion_Id]= @Tipo_Ubicacion_Id 
   AND CONVERT(DATE,[Fecha_Inicio]) = CONVERT(DATE,@Fecha_Inicio)
   AND CONVERT(DATE,[Fecha_Fin]) = CONVERT(DATE,@Fecha_Fin)
   
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](199,1,51,@Material_Material + ' vs. ' + CONVERT(VARCHAR(15),@Clave_Tipo_Ubicacion) + ' Con Fecha_Inicio ' + CONVERT(VARCHAR(25),@Fecha_Inicio) + ' Y Fecha_Fin ' + CONVERT(VARCHAR(25),@Fecha_Fin)) 
   SET @Result_ShowAlert = @Result_ShowViewMessage 
   RETURN 
END ]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
    [APP].[Material_Inventario_Teorico]
WHERE 
  [Material_Id] = @Material_Id
   AND [Tipo_Ubicacion_Id]= @Tipo_Ubicacion_Id 
   AND CONVERT(DATE,[Fecha_Inicio]) = CONVERT(DATE,@Fecha_Inicio)
   AND CONVERT(DATE,[Fecha_Fin]) = CONVERT(DATE,@Fecha_Fin)
   AND Material_Inventario_Teorico_Id <> @Material_Inventario_Teorico_Id 
   
    
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](200,1,51,@Material_Material + ' vs. ' + CONVERT(VARCHAR(15),@Clave_Tipo_Ubicacion) + ' Con Fecha_Inicio ' + CONVERT(VARCHAR(25),@Fecha_Inicio) + ' Y Fecha_Fin ' + CONVERT(VARCHAR(25),@Fecha_Fin))
   SET @Result_ShowAlert = @Result_ShowViewMessage 
   RETURN 
END 

]]></rule>
