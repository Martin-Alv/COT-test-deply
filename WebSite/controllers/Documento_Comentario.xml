    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Documento_Comentario"."Documento_Comentario_Id" "Documento_Comentario_Id"
	,"Documento_Comentario"."Orden_Produccion_Id" "Orden_Produccion_Id"
	,"Orden_Produccion"."Orden_Produccion_Id" "Orden_Produccion"
	,"Documento_Comentario"."Orden_Produccion_Detalle_Id" "Orden_Produccion_Detalle_Id"
	,"Orden_Produccion_Detalle"."Orden_Produccion_Detalle_Id" "Orden_Produccion_Detalle"
	,"Documento_Comentario"."Producto_Id" "Producto_Id"
	,"Producto"."Producto_Clave" "Producto"
	,"Documento_Comentario"."Material_Id" "Material_Id"
	,"Material"."Material" "Material"
	,"Documento_Comentario"."Solicitud_Id" "Solicitud_Id"
	,"Solicitud_"."Solicitud_Id" "Solicitud"
	,"Documento_Comentario"."Comentarios" "Comentarios"
	,"Documento_Comentario"."Picture" "Picture"
	,"Documento_Comentario"."PictureFileName" "PictureFileName"
	,"Documento_Comentario"."PictureContentType" "PictureContentType"
	,"Documento_Comentario"."PictureLength" "PictureLength"
	,"Documento_Comentario"."Es_Comentario" "Es_Comentario"
	,"Documento_Comentario"."Tipo_Comentario_Id" "Tipo_Comentario_Id"
	,"Tipo_Comentario_"."Tipo_Comentario" "Tipo_Comentario"
	,"Documento_Comentario"."Es_Imagen" "Es_Imagen"
	,"Documento_Comentario"."Tipo_Archivo_Id" "Tipo_Archivo_Id"
	,"Tipo_Archivo_"."Tipo_Archivo" "Tipo_Archivo"
	,"Documento_Comentario"."Es_Motivo_Cancelacion" "Es_Motivo_Cancelacion"
	,"Documento_Comentario"."Motivo_Cancelacion_Id" "Motivo_Cancelacion_Id"
	,"Motivo_Cancelacion"."Motivo_Cancelacion" "Motivo_Cancelacion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Documento_Comentario"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Documento_Comentario"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Documento_Comentario"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Documento_Comentario"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Documento_Comentario"."Ruta_Optima_Id" "Ruta_Optima_Id"
	,"Ruta_Optima"."Ruta_Optima_Id" "Ruta_Optima"
	,"Documento_Comentario"."Material_Surtido_Id" "Material_Surtido_Id"
	,"Material_Surtido"."Material_Surtido_Id" "Material_Surtido"
	,"Documento_Comentario"."Tipo_Movimiento_Id" "Tipo_Movimiento_Id"
	,"Tipo_Movimiento"."Tipo_Movimiento" "Tipo_Movimiento"
	,"Tipo_Movimiento"."Es_Solicitud_Devolucion" "Tipo_Movimiento_Es_Solicitud_Devolucion"
	,"Tipo_Movimiento"."Es_Solicitud_Transferencia" "Tipo_Movimiento_Es_Solicitud_Transferencia"
	,"Documento_Comentario"."Motivo_Movimiento_Id" "Motivo_Movimiento_Id"
	,"Motivo_Movimiento"."Motivo_Movimiento" "Motivo_Movimiento"
	,"Motivo_Movimiento"."Aplica_A_Devolucion" "Motivo_Movimiento_Aplica_A_Devolucion"
	,"Motivo_Movimiento"."Aplica_A_Transferencia" "Motivo_Movimiento_Aplica_A_Transferencia"
	,"Documento_Comentario"."Movimiento_Ruta_Optima_Id" "Movimiento_Ruta_Optima_Id"
	,"Movimiento_Ruta_Optima"."Numero_Grupo_Ruta_Optima" "Numero_Grupo_Ruta_Optima"
from "APP"."Documento_Comentario" "Documento_Comentario"
	left join "APP"."Solicitud" "Solicitud_" on "Documento_Comentario"."Solicitud_Id" = "Solicitud_"."Solicitud_Id"
	left join "APP"."Tipo_Comentario" "Tipo_Comentario_" on "Documento_Comentario"."Tipo_Comentario_Id" = "Tipo_Comentario_"."Tipo_Comentario_Id"
	left join "APP"."Tipo_Archivo" "Tipo_Archivo_" on "Documento_Comentario"."Tipo_Archivo_Id" = "Tipo_Archivo_"."Tipo_Archivo_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Documento_Comentario"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Documento_Comentario"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Documento_Comentario"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Documento_Comentario"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
	left join "dbo"."Orden_Produccion" "Orden_Produccion" on "Documento_Comentario"."Orden_Produccion_Id" = "Orden_Produccion"."Orden_Produccion_Id"
	left join "dbo"."Orden_Produccion_Detalle" "Orden_Produccion_Detalle" on "Documento_Comentario"."Orden_Produccion_Detalle_Id" = "Orden_Produccion_Detalle"."Orden_Produccion_Detalle_Id"
	left join "APP"."Producto" "Producto" on "Documento_Comentario"."Producto_Id" = "Producto"."Producto_Id"
	left join "APP"."Material" "Material" on "Documento_Comentario"."Material_Id" = "Material"."Material_Id"
	left join "APP"."Ruta_Optima" "Ruta_Optima" on "Documento_Comentario"."Ruta_Optima_Id" = "Ruta_Optima"."Ruta_Optima_Id"
	left join "APP"."Motivo_Cancelacion" "Motivo_Cancelacion" on "Documento_Comentario"."Motivo_Cancelacion_Id" = "Motivo_Cancelacion"."Motivo_Cancelacion_Id"
	left join "APP"."Material_Surtido" "Material_Surtido" on "Documento_Comentario"."Material_Surtido_Id" = "Material_Surtido"."Material_Surtido_Id"
	left join "APP"."Tipo_Movimiento" "Tipo_Movimiento" on "Documento_Comentario"."Tipo_Movimiento_Id" = "Tipo_Movimiento"."Tipo_Movimiento_Id"
	left join "APP"."Motivo_Movimiento" "Motivo_Movimiento" on "Documento_Comentario"."Motivo_Movimiento_Id" = "Motivo_Movimiento"."Motivo_Movimiento_Id"
	left join "APP"."Movimiento_Ruta_Optima" "Movimiento_Ruta_Optima" on "Documento_Comentario"."Movimiento_Ruta_Optima_Id" = "Movimiento_Ruta_Optima"."Movimiento_Ruta_Optima_Id"
]]></text>
    </command>
