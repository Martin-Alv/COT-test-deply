<?xml version="1.0" encoding="utf-8"?>
<dataController name="Orden_Detalle1" label="Orden_Detalle1" conflictDetection="overwriteChanges" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Orden_Detalle"."Orden_Detalle_Id" "Orden_Detalle_Id"
	,"Orden_Detalle"."Orden_Id" "Orden_Id"
	,"Orden_"."Numero_Orden" "Numero_Orden"
	,"Orden_Detalle"."Material_Id" "Material_Id"
	,"Material_"."Material_Descripcion" "Material_Descripcion"
	,"Material_"."Stock" "Material_Stock"
	,"Orden_Detalle"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Orden_Detalle"."Unidad_Material_Id" "Unidad_Material_Id"
	,"Unidad_Material_"."Unidad_Material" "Unidad_Material"
	,"Orden_Detalle"."Cantidad" "Cantidad"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Orden_Detalle"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Orden_Detalle"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Orden_Detalle"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Orden_Detalle"."Descripcion" "Descripcion"
from "dbo"."Orden_Detalle" "Orden_Detalle"
	left join "dbo"."Orden" "Orden_" on "Orden_Detalle"."Orden_Id" = "Orden_"."Orden_Id"
	left join "APP"."Material" "Material_" on "Orden_Detalle"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Familia" "Familia_" on "Orden_Detalle"."Familia_Id" = "Familia_"."Familia_Id"
	left join "APP"."Unidad_Material" "Unidad_Material_" on "Orden_Detalle"."Unidad_Material_Id" = "Unidad_Material_"."Unidad_Material_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Orden_Detalle"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Orden_Detalle"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Orden_Detalle"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
    <command id="Orden_Detalle_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Orden_Detalle_Id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Orden_Detalle_Id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Orden Detalle Id" readOnly="true" />
    <field name="Orden_Id" type="Int32" allowNulls="false" default="((0))" label="Orden Id" showInSummary="true">
      <items style="Lookup" dataController="Orden" dataValueField="Orden_Id" dataTextField="Numero_Orden" dataView="Orden_LookUp_List" pageSize="15" />
    </field>
    <field name="Numero_Orden" type="Int32" label="Numero Orden" readOnly="true" />
    <field name="Material_Id" type="Int32" allowNulls="false" default="((0))" label="Material Id" showInSummary="true">
      <items style="Lookup" dataController="Material" dataValueField="Material_Id" dataTextField="Material_Descripcion" copy="Material_Stock=Stock" dataView="Material_LookUp_List" pageSize="15" />
    </field>
    <field name="Material_Descripcion" type="String" readOnly="true" label="Material Descripcion" length="250" />
    <field name="Material_Stock" type="Decimal" readOnly="true" label="Material Stock" />
    <field name="Familia_Id" type="Int32" allowNulls="false" default="((0))" label="Familia Id" showInSummary="true">
      <items style="Lookup" dataController="Familia" newDataView="createForm1" dataValueField="Familia_Id" dataTextField="Familia_Descripcion" dataView="Familia_LookUp_List" pageSize="15" />
    </field>
    <field name="Familia_Descripcion" type="String" readOnly="true" label="Familia Descripcion" length="250" />
    <field name="Unidad_Material_Id" type="Int32" allowNulls="false" default="((0))" label="Unidad Material Id" showInSummary="true">
      <items style="Lookup" dataController="Unidad_Material" newDataView="createForm1" dataValueField="Unidad_Material_Id" dataTextField="Unidad_Material" dataView="Unidad_Material_LookUp_List" />
    </field>
    <field name="Unidad_Material" type="String" readOnly="true" label="Unidad Material" length="250" />
    <field name="Cantidad" type="Decimal" allowNulls="false" default="((0))" label="Cantidad" showInSummary="true" />
    <field name="Creado_Por" type="String" readOnly="true" label="Creado Por" length="256" />
    <field name="Fecha_Creacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Creacion" />
    <field name="Actualizado_Por" type="String" readOnly="true" label="Actualizado Por" length="256" />
    <field name="Fecha_Ultima_Actualizacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Ultima Actualizacion" />
    <field name="Estatus_Registro_Id" type="Int32" allowNulls="false" default="((1))" label="Estatus Registro Id">
      <items style="Lookup" dataController="Estatus_Registro" dataValueField="Estatus_Registro_Id" dataTextField="Estatus_Registro" dataView="Estatus_Registro_LookUp_List" pageSize="15" />
    </field>
    <field name="Estatus_Registro" type="String" readOnly="true" label="Estatus Registro" length="150" />
    <field name="Descripcion" type="String" label="Descripcion" length="150" />
  </fields>
