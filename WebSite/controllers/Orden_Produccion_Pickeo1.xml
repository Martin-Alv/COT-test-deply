from "dbo"."Orden_Produccion_Pickeo" "Orden_Produccion_Pickeo"
	left join "APP"."Area_Servicio" "Area_Servicio_" on "Orden_Produccion_Pickeo"."Area_Servicio_Id" = "Area_Servicio_"."Area_Servicio_Id"
