	left join "dbo"."Usuario_V" "Creado_Por" on "Orden_Produccion_Pickeo"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Orden_Produccion_Pickeo"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Enviado_Por_Solicitante" on "Orden_Produccion_Pickeo"."Enviado_Por_Solicitante_Id" = "Enviado_Por_Solicitante"."Usuario_Sistema_Id"
