    <rule id="r101" commandName="Custom" commandArgument="ExecJS01" type="JavaScript" phase="After" name="Valida_Y_Llama_A_ExecCS01"><![CDATA[//this.preventDefault();
// Obtenemos el valor
var valor = $("#Cantidad_Material_A_Solicitar").val();
if(valor > 0){
  [Cantidad_Mat_A_Solicitar] = valor;
  if(debug == 1){
  		console.log("Valor: " + valor);
  		console.log("Cantidad_Mat_A_Solicitar: " + [Cantidad_Mat_A_Solicitar]);
	}
}

if([Cantidad_Mat_A_Solicitar] == 0){
	this.preventDefault();
  	alertify.alert("Ingrese cantidad de mat. válido para solicitar").setHeader("Mensaje");   	
  	$("#Cantidad_Material_A_Solicitar").focus();
}
else 
if(valor > [Cantidad_Mat_Restante_Por_Solicitar]){
  if(debug == 1){
  console.log("> Valor: " + valor);
  console.log("Cantidad_Mat_Restante_Por_Solicitar: " + [Cantidad_Mat_Restante_Por_Solicitar]);
}
  this.preventDefault();
  	alertify.alert("La cantidad a solicitar no debe ser mayor a la cantidad restante por solicitar.").setHeader("Mensaje");   	;
  	$("#Cantidad_Material_A_Solicitar").focus();
}
else
	{
      // Ejecutamos BR ExecCS01
      $app.execute({
        controller: 'Grid_Orden_Detalle_De_Orden_Produccion_V'
        , command: 'Custom'
        , argument: 'ExecCS01'
        , values: [
              { name: 'Orden_Produccion_Detalle_Id', newValue: [Orden_Produccion_Detalle_Id] }
            ,{ name: 'Cantidad_Mat_A_Solicitar', newValue: [Cantidad_Mat_A_Solicitar] }      
          ,{ name: 'Area_Entrega_Id', newValue: [Area_Entrega_Id] } 
          ]
        ,done: function (result) {
          eval(result.clientScript); 
          if(debug == 1){
            console.log("ExecCS01 result : ");
          	console.log(result);      
          }           
        }
      })
	}
  ]]></rule>
