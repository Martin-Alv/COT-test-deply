    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Area_Servicio_Familia"."Area_Servicio_Familia_Id" "Area_Servicio_Familia_Id"
	,"Area_Servicio_Familia"."Area_Servicio_Id" "Area_Servicio_Id"
	,"Area_Servicio_"."Area_Servicio" "Area_Servicio"
	,"Area_Servicio_Familia"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Clave" "Familia_Clave"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Area_Servicio_Familia"."Categoria_Id" "Categoria_Id"
	,"Categoria_"."Categoria_Clave" "Categoria_Clave"
	,"Categoria_"."Categoria_Descripcion" "Categoria_Descripcion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Area_Servicio_Familia"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Area_Servicio_Familia"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Area_Servicio_Familia"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Area_Servicio_Familia" "Area_Servicio_Familia"
	left join "APP"."Area_Servicio" "Area_Servicio_" on "Area_Servicio_Familia"."Area_Servicio_Id" = "Area_Servicio_"."Area_Servicio_Id"
	left join "APP"."Familia" "Familia_" on "Area_Servicio_Familia"."Familia_Id" = "Familia_"."Familia_Id"
	left join "APP"."Categoria" "Categoria_" on "Area_Servicio_Familia"."Categoria_Id" = "Categoria_"."Categoria_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Area_Servicio_Familia"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Area_Servicio_Familia"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Area_Servicio_Familia"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
