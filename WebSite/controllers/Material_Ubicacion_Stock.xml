  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Material_Ubicacion_Stock"."Material_Ubicacion_Stock_Id" "Material_Ubicacion_Stock_Id"
	,"Material_Ubicacion_Stock"."Material_Id" "Material_Id"
	,"Material_"."Material" "Material"
	,"Material_Ubicacion_Stock"."Tipo_Almacenamiento_Id" "Tipo_Almacenamiento_Id"
	,"Tipo_Almacenamiento_"."Tipo_Almacenamiento" "Tipo_Almacenamiento"
	,"Material_Ubicacion_Stock"."Ubicacion_Almacenamiento_Id" "Ubicacion_Almacenamiento_Id"
	,"Ubicacion_Almacenamiento_"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	,"Material_Ubicacion_Stock"."Total_Stock" "Total_Stock"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Material_Ubicacion_Stock"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Material_Ubicacion_Stock"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Material_Ubicacion_Stock"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Material_Ubicacion_Stock"."Til" "Til"
	,"Material_Ubicacion_Stock"."Tipo_Ubicacion_Id" "Tipo_Ubicacion_Id"
	,"Tipo_Ubicacion"."Clave_Tipo_Ubicacion" "Clave_Tipo_Ubicacion"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Material_Ubicacion_Stock"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Material_Ubicacion_Stock"."PO" "PO"
from "APP"."Material_Ubicacion_Stock" "Material_Ubicacion_Stock"
	left join "APP"."Material" "Material_" on "Material_Ubicacion_Stock"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Tipo_Almacenamiento" "Tipo_Almacenamiento_" on "Material_Ubicacion_Stock"."Tipo_Almacenamiento_Id" = "Tipo_Almacenamiento_"."Tipo_Almacenamiento_Id"
	left join "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento_" on "Material_Ubicacion_Stock"."Ubicacion_Almacenamiento_Id" = "Ubicacion_Almacenamiento_"."Ubicacion_Almacenamiento_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material_Ubicacion_Stock"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material_Ubicacion_Stock"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material_Ubicacion_Stock"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Tipo_Ubicacion" "Tipo_Ubicacion" on "Material_Ubicacion_Stock"."Tipo_Ubicacion_Id" = "Tipo_Ubicacion"."Tipo_Ubicacion_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Material_Ubicacion_Stock"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
]]></text>
    </command>
