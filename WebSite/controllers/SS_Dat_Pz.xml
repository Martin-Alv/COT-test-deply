    <command id="command1" type="Text">
      <text><![CDATA[
select
	"SS_Dat_Pz"."SS_Dat_Pz_Id" "SS_Dat_Pz_Id"
	,"SS_Dat_Pz"."Fecha_Inicio" "Fecha_Inicio"
	,"SS_Dat_Pz"."Fecha_Fin" "Fecha_Fin"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"SS_Dat_Pz"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"SS_Dat_Pz"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"SS_Dat_Pz"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"SS_Dat_Pz"."Orden_Produccion_Id" "Orden_Produccion_Id"
	,"Orden_Produccion"."Numero_Orden_Produccion" "Numero_Orden_Produccion"
	,"SS_Dat_Pz"."Celda_Id" "Celda_Id"
	,"Celda"."Celda_Descripcion" "Celda_Descripcion"
	,"SS_Dat_Pz"."Turno_Laboral_Id" "Turno_Laboral_Id"
	,"Turno_Laboral"."Turno_Laboral" "Turno_Laboral"
from "APP"."SS_Dat_Pz" "SS_Dat_Pz"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "SS_Dat_Pz"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "SS_Dat_Pz"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "SS_Dat_Pz"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Celda" "Celda" on "SS_Dat_Pz"."Celda_Id" = "Celda"."Celda_Id"
	left join "APP"."Turno_Laboral" "Turno_Laboral" on "SS_Dat_Pz"."Turno_Laboral_Id" = "Turno_Laboral"."Turno_Laboral_Id"
	left join "dbo"."Orden_Produccion" "Orden_Produccion" on "SS_Dat_Pz"."Orden_Produccion_Id" = "Orden_Produccion"."Orden_Produccion_Id"
]]></text>
    </command>
