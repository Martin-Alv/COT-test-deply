  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Desviacion_Material
WHERE
	Orden_Produccion_Id = @Orden_Produccion_Detalle
	AND Producto_Id = @Producto_Id
	AND Material_Id = @Material_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](63,1,21,CONVERT(VARCHAR(20),@Orden_Produccion_Detalle_Id) + ' vs. ' + @Producto_Descripcion + ' vs. ' + @Material)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](63,1,21,CONVERT(VARCHAR(20),@Orden_Produccion_Detalle_Id)  + ' vs. ' + @Producto_Descripcion + ' vs. ' + @Material)
   RETURN
END 
]]></rule>
    <rule id="r102" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE 
   APP.Desviacion_Material
SET
    Creado_Por_Id = @BusinessRules_UserId
   ,Fecha_Creacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
   ,Actualizado_Por_Id = @BusinessRules_UserId
   ,Fecha_Ultima_Actualizacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
WHERE
   Desviacion_Material_Id = @Desviacion_Material_Id]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Desviacion_Material
WHERE
	Orden_Produccion_Id = @Orden_Produccion_Id
	AND Producto_Id = @Producto_Id
	AND Material_Id = @Material_Id
	AND Desviacion_Material_Id <> @Desviacion_Material_Id
	
IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](64,1,21,CONVERT(VARCHAR(20),@Numero_Orden_Produccion) + ' vs. ' + @Producto_Descripcion + ' vs. ' + @Material)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](64,1,21,CONVERT(VARCHAR(20),@Numero_Orden_Produccion)  + ' vs. ' + @Producto_Descripcion + ' vs. ' + @Material)
   RETURN
END 
]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="After"><![CDATA[UPDATE 
