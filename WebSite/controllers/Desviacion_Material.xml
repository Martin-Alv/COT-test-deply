  <commands>
    <command id="command1" type="Text" custom="false">
      <text><![CDATA[select
	"Desviacion_Material"."Desviacion_Material_Id" "Desviacion_Material_Id"
	,"Desviacion_Material"."Orden_Produccion_Id" "Orden_Produccion_Id"
	,"Orden_Produccion_"."Numero_Orden_Produccion" "Numero_Orden_Produccion"
	,"Desviacion_Material"."Orden_Produccion_Detalle_Id" "Orden_Produccion_Detalle_Id"
	,"Orden_Produccion_Detalle"."Orden_Produccion_Detalle_Id" "Orden_Produccion_Detalle"
	,"Desviacion_Material"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Desviacion_Material"."Producto_Id" "Producto_Id"
	,"Producto_"."Producto_Descripcion" "Producto_Descripcion"
	,"Desviacion_Material"."Material_Id" "Material_Id"
	,"Material_"."Material" "Material"
	,"Desviacion_Material"."Categoria_Desviacion_Id" "Categoria_Desviacion_Id"
	,"Categoria_Desviacion_"."Categoria_Desviacion" "Categoria_Desviacion"
	,"Desviacion_Material"."Folio_Desviacion" "Folio_Desviacion"
	,"Desviacion_Material"."Cantidad_Material_Sustento" "Cantidad_Material_Sustento"
	,"Desviacion_Material"."Fecha_Creacion" "Fecha_Creacion"
	,"Desviacion_Material"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Desviacion_Material"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
from "APP"."Desviacion_Material" "Desviacion_Material"
	left join "dbo"."Orden_Produccion" "Orden_Produccion_" on "Desviacion_Material"."Orden_Produccion_Id" = "Orden_Produccion_"."Orden_Produccion_Id"
	left join "APP"."Familia" "Familia_" on "Desviacion_Material"."Familia_Id" = "Familia_"."Familia_Id"
	left join "APP"."Producto" "Producto_" on "Desviacion_Material"."Producto_Id" = "Producto_"."Producto_Id"
	left join "APP"."Material" "Material_" on "Desviacion_Material"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Categoria_Desviacion" "Categoria_Desviacion_" on "Desviacion_Material"."Categoria_Desviacion_Id" = "Categoria_Desviacion_"."Categoria_Desviacion_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Desviacion_Material"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Desviacion_Material"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Desviacion_Material"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Orden_Produccion_Detalle" "Orden_Produccion_Detalle" on "Desviacion_Material"."Orden_Produccion_Detalle_Id" = "Orden_Produccion_Detalle"."Orden_Produccion_Detalle_Id"
]]></text>
    </command>
