select
	"Movimiento_Desglose"."Movimiento_Desglose_Id" "Movimiento_Desglose_Id"
	,"Movimiento_Desglose"."Material_Ubicacion_Almacenamiento_De_Surtido_Id" "Material_Ubicacion_Almacenamiento_De_Surtido_Id"
