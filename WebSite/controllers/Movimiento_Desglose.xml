	,"Material"."Material" "Material"
	,"Material"."Material_Descripcion" "Material_Descripcion"
	,"Movimiento_Desglose"."Material_Ubicacion_Stock_Origen_Id" "Material_Ubicacion_Stock_Origen_Id"
	,"Movimiento_Desglose"."Ubicacion_Almacenamiento_De_Surtido_Id" "Ubicacion_Almacenamiento_De_Surtido_Id"
	,"Ubicacion_Almacenamiento"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	,"Movimiento_Desglose"."Cantidad" "Cantidad"
	,"Movimiento_Desglose"."Unidad_Material_Entregado_Id" "Unidad_Material_Entregado_Id"
	,"Unidad_Material"."Unidad_Material" "Unidad_Material"
	,"Movimiento_Desglose"."Solicitud_Id" "Solicitud_Id"
	,"Movimiento_Desglose"."Material_Ubicacion_Stock_Destino_Id" "Material_Ubicacion_Stock_Destino_Id"
	,"Movimiento_Desglose"."Destino_Id" "Destino_Id"
	,"Movimiento_Desglose"."Destino" "Destino"
	,"Movimiento_Desglose"."Caso" "Caso"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Movimiento_Desglose"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Movimiento_Desglose"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Movimiento_Desglose"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Movimiento_Desglose" "Movimiento_Desglose"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Movimiento_Desglose"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "APP"."Material" "Material" on "Movimiento_Desglose"."Material_Id" = "Material"."Material_Id"
	left join "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento" on "Movimiento_Desglose"."Ubicacion_Almacenamiento_De_Surtido_Id" = "Ubicacion_Almacenamiento"."Ubicacion_Almacenamiento_Id"
	left join "APP"."Unidad_Material" "Unidad_Material" on "Movimiento_Desglose"."Unidad_Material_Entregado_Id" = "Unidad_Material"."Unidad_Material_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Movimiento_Desglose"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Movimiento_Desglose"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
