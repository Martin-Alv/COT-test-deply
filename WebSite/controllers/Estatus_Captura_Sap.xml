  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Estatus_Captura_Sap] 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Estatus_Captura_Sap))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Estatus_Captura_Sap)) 

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](232,1,66,@Estatus_Captura_Sap) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](232,1,66,@Estatus_Captura_Sap) 
   RETURN 
END ]]></rule>
