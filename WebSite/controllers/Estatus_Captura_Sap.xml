	,"Estatus_Captura_Sap"."Significa_Capturado_Exitosamente" "Significa_Capturado_Exitosamente"
	,"Estatus_Captura_Sap"."Significa_Capturado_Con_Errores" "Significa_Capturado_Con_Errores"
	,"Estatus_Captura_Sap"."Significa_Cancelado" "Significa_Cancelado"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Estatus_Captura_Sap"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Estatus_Captura_Sap"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Estatus_Captura_Sap"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Estatus_Captura_Sap" "Estatus_Captura_Sap"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Estatus_Captura_Sap"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Estatus_Captura_Sap"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Estatus_Captura_Sap"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
