    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Estatus_Captura_Sap"."Estatus_Captura_Sap_Id" "Estatus_Captura_Sap_Id"
	,"Estatus_Captura_Sap"."Estatus_Captura_Sap" "Estatus_Captura_Sap"
