    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Documento_Material"."Documento_Material_Id" "Documento_Material_Id"
	,"Documento_Material"."PictureFileName" "PictureFileName"
	,"Documento_Material"."PictureContentType" "PictureContentType"
	,"Documento_Material"."PictureLength" "PictureLength"
	,"Documento_Material"."Notas" "Notas"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Documento_Material"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Documento_Material"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Documento_Material"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Documento_Material"."Material_Id" "Material_Id"
	,"Material"."Material" "Material"
	,"Documento_Material"."Nombre_Documento" "Nombre_Documento"
from "APP"."Documento_Material" "Documento_Material"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Documento_Material"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Documento_Material"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Documento_Material"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Material" "Material" on "Documento_Material"."Material_Id" = "Material"."Material_Id"
]]></text>
    </command>
