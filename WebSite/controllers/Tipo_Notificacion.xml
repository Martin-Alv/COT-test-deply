  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Tipo_Notificacion]
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Notificacion))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Notificacion)) 

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](201,1,52,@Tipo_Notificacion) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](201,1,52,@Tipo_Notificacion) 
   RETURN 
END]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 

   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Tipo_Notificacion]
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Notificacion))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Notificacion)) 
AND Tipo_Notificacion_Id <> @Tipo_Notificacion_Id  

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](202,1,52,@Tipo_Notificacion) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](202,1,52,@Tipo_Notificacion) 
   RETURN 
END ]]></rule>
