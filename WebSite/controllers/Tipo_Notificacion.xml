select
	"Tipo_Notificacion"."Tipo_Notificacion_Id" "Tipo_Notificacion_Id"
	,"Tipo_Notificacion"."Tipo_Notificacion" "Tipo_Notificacion"
	,"Tipo_Notificacion"."Es_Notificacion_OPGI" "Es_Notificacion_OPGI"
	,"Tipo_Notificacion"."Es_Notificacion_Inventario_Piso" "Es_Notificacion_Inventario_Piso"
	,"Tipo_Notificacion"."Enviar" "Enviar"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Tipo_Notificacion"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Tipo_Notificacion"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Notificacion"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_"
from "APP"."Tipo_Notificacion" "Tipo_Notificacion"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Notificacion"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Notificacion"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Notificacion"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
