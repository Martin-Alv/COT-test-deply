    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Usuario_Aplicacion_Familia"."Usuario_Aplicacion_Id" "Usuario_Aplicacion_Id"
	,"Usuario_Aplicacion"."Usuario_Sistema" "Usuario_Aplicacion"
	,"Usuario_Aplicacion_Familia"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Usuario_Aplicacion_Familia"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Usuario_Aplicacion_Familia"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Usuario_Aplicacion_Familia"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Usuario_Aplicacion_Familia"."Usuario_Aplicacion_Familia_Id" "Usuario_Aplicacion_Familia_Id"
from "APP"."Usuario_Aplicacion_Familia" "Usuario_Aplicacion_Familia"
	left join "APP"."Familia" "Familia_" on "Usuario_Aplicacion_Familia"."Familia_Id" = "Familia_"."Familia_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Usuario_Aplicacion_Familia"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Usuario_Aplicacion_Familia"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Usuario_Aplicacion_Familia"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Usuario_Aplicacion" on "Usuario_Aplicacion_Familia"."Usuario_Aplicacion_Id" = "Usuario_Aplicacion"."Usuario_Sistema_Id"
]]></text>
    </command>
