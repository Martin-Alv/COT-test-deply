  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Razon_Tiempo_Muerto] 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Razon_Tiempo_Muerto))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Razon_Tiempo_Muerto)) 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](212,1,58,@Razon_Tiempo_Muerto) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](212,1,58,@Razon_Tiempo_Muerto) 
   RETURN 
END
]]></rule>
