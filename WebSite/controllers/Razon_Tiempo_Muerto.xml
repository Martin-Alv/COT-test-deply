from "APP"."Razon_Tiempo_Muerto" "Razon_Tiempo_Muerto"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Razon_Tiempo_Muerto"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
