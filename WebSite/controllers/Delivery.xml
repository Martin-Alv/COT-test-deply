<?xml version="1.0" encoding="utf-8"?>
<dataController name="Delivery" conflictDetection="overwriteChanges" label="Delivery" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Delivery"."Delivery_Id" "Delivery_Id"
	,"Delivery"."Delivery" "Delivery"
	,"Delivery"."Sales_Order" "Sales_Order"
	,"Delivery"."Sales_Item" "Sales_Item"
	,"Delivery"."Producto_Id" "Producto_Id"
	,"Producto_"."Producto_Clave" "Producto_Clave"
	,"Delivery"."Cantidad" "Cantidad"
	,"Delivery"."Es_Negativo_En_Sap" "Es_Negativo_En_Sap"
	,"Delivery"."Valor_Real_Reporte_Sap" "Valor_Real_Reporte_Sap"
	,"Delivery"."Planta_Id" "Planta_Id"
	,"Planta"."Numero_Planta" "Numero_Planta"
	,"Delivery"."Almacen_Id" "Almacen_Id"
	,"Almacen"."Numero_Almacen" "Almacen"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Delivery"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Delivery"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Delivery"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Delivery" "Delivery"
	left join "APP"."Producto" "Producto_" on "Delivery"."Producto_Id" = "Producto_"."Producto_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Delivery"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Delivery"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Delivery"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Planta" "Planta" on "Delivery"."Planta_Id" = "Planta"."Planta_Id"
	left join "APP"."Almacen" "Almacen" on "Delivery"."Almacen_Id" = "Almacen"."Almacen_Id"
]]></text>
    </command>
    <command id="Delivery_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Delivery_Id" />
      </output>
    </command>
  </commands>
