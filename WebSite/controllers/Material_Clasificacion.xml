  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Material_Clasificacion"."Material_Clasificacion_Id" "Material_Clasificacion_Id"
	,"Material_Clasificacion"."Material_Clasificacion" "Material_Clasificacion"
	,"Material_Clasificacion"."Materal_Descripcion" "Materal_Descripcion"
	,"Material_Clasificacion"."MRP_Type_Id" "MRP_Type_Id"
	,"MRP_Type_"."MRP_Type" "MRP_Type_MRP_Type"
	,"Material_Clasificacion"."Clasificacion" "Clasificacion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Material_Clasificacion"."Fecha_Creacion" "Fecha_Creacion"
	,"Material_Clasificacion"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Material_Clasificacion"."Estatus_Registro_Id" "Estatus_Registro_Id"
from "APP"."Material_Clasificacion" "Material_Clasificacion"
	left join "dbo"."MRP_Type" "MRP_Type_" on "Material_Clasificacion"."MRP_Type_Id" = "MRP_Type_"."MRP_Type_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material_Clasificacion"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material_Clasificacion"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material_Clasificacion"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
    <command id="Material_Clasificacion_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Material_Clasificacion_Id" />
      </output>
    </command>
  </commands>
