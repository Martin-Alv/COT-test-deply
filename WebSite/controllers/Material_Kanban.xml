    <field name="Clave_Kanban" type="String" readOnly="true" label="Clave Kanban" length="150" />
    <field name="Cantidad_Kanban" type="Decimal" allowNulls="false" default="((0))" label="Cantidad Kanban" showInSummary="true" />
