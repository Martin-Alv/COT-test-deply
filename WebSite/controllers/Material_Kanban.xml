<?xml version="1.0" encoding="utf-8"?>
<dataController name="Material_Kanban" conflictDetection="overwriteChanges" label="Material Kanban" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Material_Kanban"."Material_Kanban_Id" "Material_Kanban_Id"
	,"Material_Kanban"."Material_Id" "Material_Id"
	,"Material_"."Material" "Material"
	,"Material_Kanban"."Kanban_Id" "Kanban_Id"
	,"Kanban_"."Clave_Kanban" "Clave_Kanban"
	,"Material_Kanban"."Cantidad_Kanban" "Cantidad_Kanban"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Material_Kanban"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Material_Kanban"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Material_Kanban"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Material_Kanban" "Material_Kanban"
	left join "APP"."Material" "Material_" on "Material_Kanban"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Kanban" "Kanban_" on "Material_Kanban"."Kanban_Id" = "Kanban_"."Kanban_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material_Kanban"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material_Kanban"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material_Kanban"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
    <command id="Material_Kanban_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Material_Kanban_Id" />
      </output>
    </command>
  </commands>
