    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT
SET @Already_Exists = 0
SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Usuario_Proveedor
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Usuario_Proveedor_Id))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Usuario_Proveedor_Id))
AND Usuario_Proveedor_Id <> @Usuario_Proveedor_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1319,1,10,@Usuario_Proveedor_Id)
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1319,1,10,@Usuario_Proveedor_Id)
