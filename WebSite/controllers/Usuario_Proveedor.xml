    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Usuario_Proveedor"."Usuario_Proveedor_Id" "Usuario_Proveedor_Id"
	,"Usuario_Proveedor"."Proveedor_ERSL_Id" "Proveedor_ERSL_Id"
	,"Proveedor_ERSL_"."Proveedor_ERSL" "Proveedor_ERSL_Proveedor_ERSL"
	,"Usuario_Sistema"."Usuario_Sistema" "Usuario_Sistema"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Usuario_Proveedor"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Usuario_Proveedor"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Usuario_Proveedor"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Usuario_Proveedor"."Usuario_Sistema_Id" "Usuario_Sistema_Id"
from "APP"."Usuario_Proveedor" "Usuario_Proveedor"
	left join "APP"."Proveedor_ERSL" "Proveedor_ERSL_" on "Usuario_Proveedor"."Proveedor_ERSL_Id" = "Proveedor_ERSL_"."Proveedor_ERSL_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Usuario_Proveedor"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Usuario_Proveedor"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Usuario_Proveedor"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Usuario_Sistema" on "Usuario_Proveedor"."Usuario_Sistema_Id" = "Usuario_Sistema"."Usuario_Sistema_Id"
]]></text>
    </command>
