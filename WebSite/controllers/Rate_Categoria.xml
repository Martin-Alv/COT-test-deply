    <rule id="r101" commandName="Update" type="Sql" phase="After"><![CDATA[UPDATE 
   [APP].[Rate_Categoria]
SET
   [Actualizado_Por_Id] = @BusinessRules_UserId
   ,[Fecha_Ultima_Actualizacion] = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
WHERE
   [Rate_Categoria_Id] = @Rate_Categoria_Id]]></rule>
    <rule id="r102" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Rate_Categoria 
WHERE 
       [Categoria_Id] = @Categoria_Id
   AND [Familia_Id] = @Familia_Id   
   AND [Rate_Sap] = @Rate_Sap
   AND [Rate_Real] = @Rate_Real
   
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](210,1,57,@Categoria_Clave + ' vs. ' + @Familia_Clave  + ' vs. ' + CONVERT(VARCHAR(15),@Rate_Sap) + ' vs. ' + CONVERT(VARCHAR(15),@Rate_Real)) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](210,1,57,@Categoria_Clave + ' vs. ' + @Familia_Clave + ' vs. '+ CONVERT(VARCHAR(15),@Rate_Sap) + ' vs. ' +  CONVERT(VARCHAR(15),@Rate_Real)) 
   RETURN 
END ]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Rate_Categoria
WHERE 
          [Categoria_Id] = @Categoria_Id
   AND [Familia_Id] = @Familia_Id
   AND [Rate_Sap] = @Rate_Sap
   AND [Rate_Real] = @Rate_Real
   AND [Rate_Categoria_Id] <> @Rate_Categoria_Id
  
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](211,1,57,@Categoria_Clave + ' vs. ' + @Familia_Clave  + ' vs. ' + CONVERT(VARCHAR(15),@Rate_Sap) + ' vs. '+ CONVERT(VARCHAR(15),@Rate_Real))  
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](211,1,57,@Categoria_Clave + ' vs. ' + @Familia_Clave + ' vs. ' + CONVERT(VARCHAR(15),@Rate_Sap) + ' vs. '+ CONVERT(VARCHAR(15),@Rate_Real)) 
   RETURN 
END ]]></rule>
