  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Rate_Categoria"."Rate_Categoria_Id" "Rate_Categoria_Id"
	,"Rate_Categoria"."Categoria_Id" "Categoria_Id"
	,"Categoria"."Categoria_Clave" "Categoria_Clave"
	,"Categoria"."Categoria_Descripcion" "Categoria_Descripcion"
	,"Rate_Categoria"."Familia_Id" "Familia_Id"
	,"Familia"."Familia_Clave" "Familia_Clave"
	,"Familia"."Familia_Descripcion" "Familia_Descripcion"
	,"Rate_Categoria"."Rate_Sap" "Rate_Sap"
	,"Rate_Categoria"."Rate_Real" "Rate_Real"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Rate_Categoria"."Fecha_Creacion" "Fecha_Creacion"
	,"Rate_Categoria"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Rate_Categoria"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro"."Estatus_Registro" "Estatus_Registro"
	,"Rate_Categoria"."Id_SubCategoria" "Id_Subcategoria"
	,"RATE_SUBCATEGORIA"."SUBCATEGORIA_DESCRIPCION" "Rate_Subcategoria_Descripcion"
from "APP"."Rate_Categoria" "Rate_Categoria"
	left join "APP"."Categoria" "Categoria" on "Rate_Categoria"."Categoria_Id" = "Categoria"."Categoria_Id"
	left join "APP"."Familia" "Familia" on "Rate_Categoria"."Familia_Id" = "Familia"."Familia_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Rate_Categoria"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Rate_Categoria"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro" on "Rate_Categoria"."Estatus_Registro_Id" = "Estatus_Registro"."Estatus_Registro_Id"
	left join "APP"."RATE_SUBCATEGORIA" "RATE_SUBCATEGORIA" on "Rate_Categoria"."Id_SubCategoria" = "RATE_SUBCATEGORIA"."ID_SUBCATEGORIA"
]]></text>
    </command>
    <command id="Rate_Categoria_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Rate_Categoria_Id" />
      </output>
    </command>
  </commands>
