  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Estatus_Solicitud
WHERE
	dbo.fn_Sustituir_Caracteres_Especiales(Lower(Estatus_Solicitud))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Estatus_Solicitud))

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](59,1,19,@Estatus_Solicitud)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
    <rule id="r102" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE 
   APP.Estatus_Solicitud
SET
    Creado_Por_Id = @BusinessRules_UserId
   ,Fecha_Creacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
   ,Actualizado_Por_Id = @BusinessRules_UserId
   ,Fecha_Ultima_Actualizacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL)
WHERE
   Estatus_Solicitud_Id = @Estatus_Solicitud_Id]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Estatus_Solicitud
WHERE
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Estatus_Solicitud))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Estatus_Solicitud))
AND Estatus_Solicitud_Id <> @Estatus_Solicitud_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](60,1,19,@Estatus_Solicitud)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
