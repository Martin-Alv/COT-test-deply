    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Estatus_Solicitud"."Estatus_Solicitud_Id" "Estatus_Solicitud_Id"
	,"Estatus_Solicitud"."Estatus_Solicitud" "Estatus_Solicitud"
	,"Estatus_Solicitud"."Significa_Pendiente" "Significa_Pendiente"
	,"Estatus_Solicitud"."Significa_Entregada" "Significa_Entregada"
	,"Estatus_Solicitud"."Significa_Cancelada" "Significa_Cancelada"
	,"Estatus_Solicitud"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Estatus_Solicitud"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Estatus_Solicitud"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Estatus_Solicitud"."Significa_Enviada_A_Surtir" "Significa_Enviada_A_Surtir"
	,"Estatus_Solicitud"."Significa_Surtida_Completa" "Significa_Surtida_Completa"
	,"Estatus_Solicitud"."Significa_Surtida_Parcial" "Significa_Surtida_Parcial"
	,"Estatus_Solicitud"."Significa_Pendiente_Por_Entregar" "Significa_Pendiente_Por_Entregar"
from "APP"."Estatus_Solicitud" "Estatus_Solicitud"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Estatus_Solicitud"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Estatus_Solicitud"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Estatus_Solicitud"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
