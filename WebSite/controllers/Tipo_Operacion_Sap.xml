    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tipo_Operacion_Sap"."Tipo_Operacion_Sap_Id" "Tipo_Operacion_Sap_Id"
	,"Tipo_Operacion_Sap"."Tipo_Operacion_Sap" "Tipo_Operacion_Sap"
	,"Tipo_Operacion_Sap"."Es_Captura_De_Surtido" "Es_Captura_De_Surtido"
	,"Tipo_Operacion_Sap"."Es_Devolucion_De_Material" "Es_Devolucion_De_Material"
	,"Tipo_Operacion_Sap"."Es_Transferencia_De_Material" "Es_Transferencia_De_Material"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Tipo_Operacion_Sap"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Tipo_Operacion_Sap"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Operacion_Sap"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Tipo_Operacion_Sap"."Es_Cuadre_Inventario_Devolucion" "Es_Cuadre_Inventario_Devolucion"
	,"Tipo_Operacion_Sap"."Es_Cuadre_Inventario_Transferencia" "Es_Cuadre_Inventario_Transferencia"
from "APP"."Tipo_Operacion_Sap" "Tipo_Operacion_Sap"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Operacion_Sap"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Operacion_Sap"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Operacion_Sap"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
