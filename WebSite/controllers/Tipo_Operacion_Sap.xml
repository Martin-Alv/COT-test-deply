  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Operacion_Sap
 WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Operacion_Sap))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Operacion_Sap)) 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](208,1,59,@Tipo_Operacion_Sap) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](208,1,59,@Tipo_Operacion_Sap) 
   RETURN 
END 
]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Operacion_Sap
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Operacion_Sap))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Operacion_Sap)) 
AND Tipo_Operacion_Sap_Id <> @Tipo_Operacion_Sap_Id  
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](209,1,59,@Tipo_Operacion_Sap)  
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](209,1,59,@Tipo_Operacion_Sap)  
   RETURN 
END

]]></rule>
