	,"DC_S_Planta_"."Descripcion_Planta" "DC_S_Descripcion"
	,"Etiqueta_Embarque"."Producto_Id" "Producto_Id"
	,"Producto_"."Producto_Descripcion" "Producto_Descripcion"
