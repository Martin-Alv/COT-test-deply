	,"Etiqueta_Embarque"."Cantidad_De_Etiquetas" "Cantidad_De_Etiquetas"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Etiqueta_Embarque"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Etiqueta_Embarque"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
