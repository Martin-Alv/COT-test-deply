	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Etiqueta_Embarque"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Etiqueta_Embarque"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Etiqueta_Embarque"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Orden_Produccion" "Orden_Produccion" on "Etiqueta_Embarque"."Orden_Produccion_Id" = "Orden_Produccion"."Orden_Produccion_Id"
]]></text>
