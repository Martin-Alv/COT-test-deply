	,"Etiqueta_Embarque"."Numero_De_Bulto" "Numero_De_Bulto"
from "APP"."Etiqueta_Embarque" "Etiqueta_Embarque"
	left join "APP"."Planta" "DC_S_Planta_" on "Etiqueta_Embarque"."DC_S_Planta_Id" = "DC_S_Planta_"."Planta_Id"
