    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tipo_Material"."Tipo_Material_Id" "Tipo_Material_Id"
	,"Tipo_Material"."Tipo_Material" "Tipo_Material"
	,"Tipo_Material"."Fecha_Creacion" "Fecha_Creacion"
	,"Tipo_Material"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Material"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
from "APP"."Tipo_Material" "Tipo_Material"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Material"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Material"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Material"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
