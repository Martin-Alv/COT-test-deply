  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Material 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Material)) = dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Material)) -- Verificar si aplica campo
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1431,1,1,@Tipo_Material) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1431,1,1,@Tipo_Material) 
   RETURN 
END]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE  
   APP.Tipo_Material 
SET 
   Creado_Por_Id = @BusinessRules_UserId 
   ,Fecha_Creacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL) 
   ,Actualizado_Por_Id = @BusinessRules_UserId 
   ,Fecha_Ultima_Actualizacion = dbo.fn_Convertir_UTC_A_Fecha_Hora_Local(GETDATE(),NULL) 
WHERE 
   Tipo_Material_Id = @Tipo_Material_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Material 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Tipo_Material)) = dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Tipo_Material)) -- Verificar si aplica campo
AND Tipo_Material_Id <> @Tipo_Material_Id 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1432,1,1,@Tipo_Material) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1432,1,1,@Tipo_Material) 
   RETURN 
END]]></rule>
