  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Tipo_Ubicacion
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Clave_Tipo_Ubicacion))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Clave_Tipo_Ubicacion))
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](246,1,68,@Clave_Tipo_Ubicacion) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](246,1,68,@Clave_Tipo_Ubicacion) 
   RETURN 
END   ]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   [APP].[Tipo_Ubicacion] 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Clave_Tipo_Ubicacion))=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Clave_Tipo_Ubicacion)) 
AND Tipo_Ubicacion_Id <> @Tipo_Ubicacion_Id 
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](247,1,68,@Clave_Tipo_Ubicacion) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](247,1,68,@Clave_Tipo_Ubicacion) 
   RETURN 
END 




]]></rule>
