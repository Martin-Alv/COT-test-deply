	,"Tipo_Ubicacion"."Descripcion_Tipo_Ubicacion" "Descripcion_Tipo_Ubicacion"
	,"Tipo_Ubicacion"."Es_Almacen" "Es_Almacen"
	,"Tipo_Ubicacion"."Es_Piso" "Es_Piso"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Tipo_Ubicacion"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Tipo_Ubicacion"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Ubicacion"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Tipo_Ubicacion"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
from "APP"."Tipo_Ubicacion" "Tipo_Ubicacion"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Ubicacion"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Ubicacion"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Ubicacion"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Tipo_Ubicacion"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
]]></text>
    </command>
