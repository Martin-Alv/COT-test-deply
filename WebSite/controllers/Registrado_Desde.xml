  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Registrado_Desde"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Registrado_Desde"."Fecha_Creacion" "Fecha_Creacion"
	,"Registrado_Desde"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Registrado_Desde"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Actualizdo_Por"."Usuario_Sistema" "Actualizdo_Por"
from "APP"."Registrado_Desde" "Registrado_Desde"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Registrado_Desde"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Registrado_Desde"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizdo_Por" on "Registrado_Desde"."Actualizado_Por_Id" = "Actualizdo_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
    <command id="Registrado_Desde_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Registrado_Desde_Id" />
      </output>
    </command>
  </commands>
