	,"Solicitud_Detalle"."Solicitud_Transaccion_Id" "Solicitud_Transaccion_Id"
	,"Solicitud_Detalle"."Solicitado_Por_Id" "Solicitado_Por_Id"
	,"Solicitado_Por"."Usuario_Sistema" "Solicitado_Por"
	,"Solicitud_Detalle"."Area_Servicio_Id" "Area_Servicio_Id"
	,"Area_Servicio_"."Area_Servicio" "Area_Servicio"
	,"Solicitud_Detalle"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Solicitud_Detalle"."Producto_Id" "Producto_Id"
	,"Producto_"."Producto_Descripcion" "Producto_Descripcion"
	,"Solicitud_Detalle"."Material_Solicitado_Id" "Material_Solicitado_Id"
	,"Material_Solicitado_"."Material" "Material_Solicitado"
	,"Solicitud_Detalle"."Unidad_Material_Solicitado_Id" "Unidad_Material_Solicitado_Id"
	,"Unidad_Material_Solicitado_"."Unidad_Material" "Unidad_Material_Solicitado"
	,"Solicitud_Detalle"."Fecha_Solicitud_Detalle" "Fecha_Solicitud_Detalle"
	,"Solicitud_Detalle"."Cantidad_Solicitado" "Cantidad_Solicitado"
	,"Solicitud_Detalle"."Cantidad_Entregado" "Cantidad_Entregado"
	,"Solicitud_Detalle"."Unidad_Material_Entregado_Id" "Unidad_Material_Entregado_Id"
	,"Unidad_Material_Entregado_"."Unidad_Material" "Unidad_Material_Entregado"
	,"Solicitud_Detalle"."Fecha_Entrega" "Fecha_Entrega"
	,"Solicitud_Detalle"."Entregado_Por_Id" "Entregado_Por_Id"
	,"Entregado_Por"."Usuario_Sistema" "Entregado_Por"
	,"Solicitud_Detalle"."Fecha_Creacion" "Fecha_Creacion"
	,"Solicitud_Detalle"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Solicitud_Detalle"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Solicitud_Detalle"."Solicitud_Id" "Solicitud_Id"
	,"Solicitud_"."Solicitud_Id" "Solicitud"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
from "APP"."Solicitud_Detalle" "Solicitud_Detalle"
	left join "dbo"."Orden_Produccion" "Orden_Produccion_" on "Solicitud_Detalle"."Orden_Produccion_Id" = "Orden_Produccion_"."Orden_Produccion_Id"
	left join "dbo"."Orden_Produccion_Detalle" "Orden_Produccion_Detalle_" on "Solicitud_Detalle"."Orden_Produccion_Detalle_Id" = "Orden_Produccion_Detalle_"."Orden_Produccion_Detalle_Id"
	left join "APP"."Solicitud" "Solicitud_" on "Solicitud_Detalle"."Solicitud_Id" = "Solicitud_"."Solicitud_Id"
	left join "APP"."Area_Servicio" "Area_Servicio_" on "Solicitud_Detalle"."Area_Servicio_Id" = "Area_Servicio_"."Area_Servicio_Id"
	left join "APP"."Familia" "Familia_" on "Solicitud_Detalle"."Familia_Id" = "Familia_"."Familia_Id"
	left join "APP"."Producto" "Producto_" on "Solicitud_Detalle"."Producto_Id" = "Producto_"."Producto_Id"
	left join "APP"."Material" "Material_Solicitado_" on "Solicitud_Detalle"."Material_Solicitado_Id" = "Material_Solicitado_"."Material_Id"
	left join "APP"."Unidad_Material" "Unidad_Material_Solicitado_" on "Solicitud_Detalle"."Unidad_Material_Solicitado_Id" = "Unidad_Material_Solicitado_"."Unidad_Material_Id"
	left join "APP"."Unidad_Material" "Unidad_Material_Entregado_" on "Solicitud_Detalle"."Unidad_Material_Entregado_Id" = "Unidad_Material_Entregado_"."Unidad_Material_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Solicitud_Detalle"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Solicitud_Detalle"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Solicitud_Detalle"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_Aplicacion_V" "Solicitado_Por" on "Solicitud_Detalle"."Solicitado_Por_Id" = "Solicitado_Por"."Usuario_Id"
	left join "dbo"."Usuario_Aplicacion_V" "Entregado_Por" on "Solicitud_Detalle"."Entregado_Por_Id" = "Entregado_Por"."Usuario_Id"
]]></text>
