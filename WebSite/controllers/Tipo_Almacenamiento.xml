    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Tipo_Almacenamiento"."Tipo_Almacenamiento_Id" "Tipo_Almacenamiento_Id"
	,"Tipo_Almacenamiento"."Tipo_Almacenamiento" "Tipo_Almacenamiento"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Tipo_Almacenamiento"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Tipo_Almacenamiento"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Almacenamiento"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Tipo_Almacenamiento"."Registrado_Desde_Id" "Registrado_Desde_Id"
from "APP"."Tipo_Almacenamiento" "Tipo_Almacenamiento"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Almacenamiento"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Tipo_Almacenamiento"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Tipo_Almacenamiento"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Tipo_Almacenamiento"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
]]></text>
    </command>
    <command id="Tipo_Almacenamiento_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Tipo_Almacenamiento_Id" />
      </output>
    </command>
  </commands>
