    <rule id="r101" commandName="Update" type="Sql" phase="After" name="Actualizar_Surtido"><![CDATA[IF @Cantidad_Entregado > 0 AND @Unidad_Material_Entregado_Id > 0
BEGIN
-- EJECUCIÓN DE SP
  EXEC dbo.sp_Actualizar_Surtido
	-- Add the parameters for the stored procedure here	
	0 -- @Tipo_Operacion_Id
	,@Ruta_Optima_Id
	,@Orden_Produccion_Id
	,@Solicitud_Id	
	,@Cantidad_Entregado
	,@Unidad_Material_Entregado_Id
	------------
	,@BusinessRules_UserId-- @Creado_Por_Id 
	,'' -- @Transaccion_Id 
	,1 -- @Fijar_Bitacora --Poner 1 si se desea guardar las operaciones y resultados del SP a la tabla de dbo.Bitacora_Sistema
	,0 -- @Mostrar_Tablas_Sistema  -- Poner 1 si se desea presentar a pantalla tablas o consultas, para uso del Desarrollador
	,0 -- @Mensaje_Sistema_Id OUT   --Parametro de Entrada y/o salida, Sirve para regresas las claves de errores, advertencias o mensajes. Inicializar en 0
	,0 -- @Simular  -- Poner 1 si se desea proceder con todas las operaciones del SP pero al final "deshacer" todo lo que se hizo
	,'' -- @Trace   -- Variable para dejar "rastro" de donde "paso" las instrucciones del SP
	,0 -- @Debug  -- Poner 1 si se desea mostrar a pantalla lo que esta "haciendo" el SP (Modo Prueba)
END ]]></rule>
