	,"Orden_Produccion_Detalle"."Familia_Id" "Familia_Id"
	,"Familia_"."Familia_Descripcion" "Familia_Descripcion"
	,"Familia_"."Familia_Clave" "Familia_Clave"
	,"Orden_Produccion_Detalle"."Area_Servicio_Id" "Area_Servicio_Id"
	,"Area_Servicio_"."Area_Servicio" "Area_Servicio"
	,"Orden_Produccion_Detalle"."Unidad_Material_Id" "Unidad_Material_Id"
