from "dbo"."Orden_Produccion_Detalle" "Orden_Produccion_Detalle"
	left join "dbo"."Orden_Produccion" "Orden_Produccion_" on "Orden_Produccion_Detalle"."Orden_Produccion_Id" = "Orden_Produccion_"."Orden_Produccion_Id"
	left join "APP"."Material" "Material_" on "Orden_Produccion_Detalle"."Material_Id" = "Material_"."Material_Id"
	left join "APP"."Familia" "Familia_" on "Orden_Produccion_Detalle"."Familia_Id" = "Familia_"."Familia_Id"
	left join "APP"."Area_Servicio" "Area_Servicio_" on "Orden_Produccion_Detalle"."Area_Servicio_Id" = "Area_Servicio_"."Area_Servicio_Id"
	left join "APP"."Unidad_Material" "Unidad_Material_" on "Orden_Produccion_Detalle"."Unidad_Material_Id" = "Unidad_Material_"."Unidad_Material_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Orden_Produccion_Detalle"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Orden_Produccion_Detalle"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Orden_Produccion_Detalle"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Estatus_Orden_Produccion_Detalle" "Estatus_Orden_Produccion_Detalle" on "Orden_Produccion_Detalle"."Estatus_Orden_Produccion_Detalle_Id" = "Estatus_Orden_Produccion_Detalle"."Estatus_Orden_Produccion_Detalle_Id"
	left join "APP"."Producto" "Producto" on "Orden_Produccion_Detalle"."Producto_Id" = "Producto"."Producto_Id"
	left join "APP"."Area_Entrega" "Area_Entrega" on "Orden_Produccion_Detalle"."Area_Entrega_Id" = "Area_Entrega"."Area_Entrega_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Orden_Produccion_Detalle"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
]]></text>
