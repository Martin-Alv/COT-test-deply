<?xml version="1.0" encoding="utf-8"?>
<dataController name="Pasillo" conflictDetection="overwriteChanges" label="Pasillo" handler="eaton.Rules.PasilloBusinessRules" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Pasillo"."Pasillo_Id" "Pasillo_Id"
	,"Pasillo"."Clave_Pasillo" "Clave_Pasillo"
	,"Pasillo"."Rango_Columna_Inicio" "Rango_Columna_Inicio"
	,"Pasillo"."Rango_Columna_Fin" "Rango_Columna_Fin"
	,"Pasillo"."Rango_Nivel_Inicio" "Rango_Nivel_Inicio"
	,"Pasillo"."Rango_Nivel_Fin" "Rango_Nivel_Fin"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Pasillo"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Pasillo"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Pasillo"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Pasillo"."Valor_Pasillo" "Valor_Pasillo"
	,"Pasillo"."Pasillo_Vecino_Izquierda_Id" "Pasillo_Vecino_Izquierda_Id"
	,"pi"."Clave_Pasillo" "Clave_Pasillo_Vecino_Izquierda"
	,"Pasillo"."Pasillo_Vecino_Derecha_Id" "Pasillo_Vecino_Derecha_Id"
	,"pd"."Clave_Pasillo" "Clave_Pasillo_Vecino_Derecha"
from "APP"."Pasillo" "Pasillo"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Pasillo"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Pasillo"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Pasillo"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "APP"."Pasillo" "pi" on "Pasillo"."Pasillo_Vecino_Izquierda_Id" = "pi"."Pasillo_Id"
	left join "APP"."Pasillo" "pd" on "Pasillo"."Pasillo_Vecino_Derecha_Id" = "pd"."Pasillo_Id"
]]></text>
    </command>
    <command id="Pasillo_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Pasillo_Id" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="Pasillo_Id" type="Int32" allowNulls="false" isPrimaryKey="true" label="Pasillo Id" readOnly="true" />
    <field name="Clave_Pasillo" type="String" allowNulls="false" label="Clave Pasillo" length="150" showInSummary="true" />
    <field name="Rango_Columna_Inicio" type="Int32" allowNulls="false" default="((0))" label="Rango Columna Inicio" showInSummary="true" />
    <field name="Rango_Columna_Fin" type="Int32" allowNulls="false" default="((0))" label="Rango Columna Fin" showInSummary="true" />
    <field name="Rango_Nivel_Inicio" type="Int32" allowNulls="false" default="((0))" label="Rango Nivel Inicio" showInSummary="true" />
    <field name="Rango_Nivel_Fin" type="Int32" allowNulls="false" default="((0))" label="Rango Nivel Fin" showInSummary="true" />
    <field name="Creado_Por" type="String" readOnly="true" label="Creado Por" length="256" />
    <field name="Fecha_Creacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Creacion" />
    <field name="Actualizado_Por" type="String" readOnly="true" label="Actualizado Por" length="256" />
    <field name="Fecha_Ultima_Actualizacion" type="DateTime" allowNulls="false" dataFormatString="g" default="(getdate())" label="Fecha Ultima Actualizacion" />
    <field name="Estatus_Registro_Id" type="Int32" allowNulls="false" default="((1))" label="Estatus Registro Id">
      <items style="Lookup" dataController="Estatus_Registro" dataValueField="Estatus_Registro_Id" dataTextField="Estatus_Registro" dataView="Estatus_Registro_LookUp_List" pageSize="15" />
    </field>
    <field name="Estatus_Registro" type="String" readOnly="true" label="Estatus Registro" length="150" />
    <field name="Valor_Pasillo" type="Int32" allowNulls="false" default="((0))" label="Valor Pasillo" />
    <field name="Pasillo_Vecino_Izquierda_Id" type="Int32" allowNulls="false" default="((0))" label="Pasillo Vecino Izquierda Id">
      <items style="Lookup" dataController="Pasillo" newDataView="createForm1" searchOnStart="true" dataValueField="Pasillo_Id" dataTextField="Clave_Pasillo" />
    </field>
    <field name="Clave_Pasillo_Vecino_Izquierda" type="String" readOnly="true" label="Clave Pasillo Vecino Izquierda" length="150" />
    <field name="Pasillo_Vecino_Derecha_Id" type="Int32" allowNulls="false" default="((0))" label="Pasillo Vecino Derecha Id">
      <items style="Lookup" dataController="Pasillo" newDataView="createForm1" searchOnStart="true" dataValueField="Pasillo_Id" dataTextField="Clave_Pasillo" />
    </field>
    <field name="Clave_Pasillo_Vecino_Derecha" type="String" readOnly="true" label="Clave Pasillo Vecino Derecha" length="150" />
  </fields>
