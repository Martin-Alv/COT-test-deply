  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Material"."Material_Id" "Material_Id"
	,"Material"."Material_Descripcion" "Material_Descripcion"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Material"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Material"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Material"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
	,"Material"."Material" "Material"
	,"MRP_Type"."MRP_Type" "Mrp_Type"
	,"Material"."MRP_Type_Id" "Mrp_Type_Id"
	,"Material"."Clasificacion_Id" "Clasificacion_Id"
	,"Clasificacion"."Clasificacion" "Clasificacion"
	,"Clasificacion"."Es_Material_Indirecto" "Clasificacion_es_Material_Indirecto"
	,"Registrado_Desde"."Registrado_Desde" "Registrado_Desde"
	,"Material"."Registrado_Desde_Id" "Registrado_Desde_Id"
	,"Material_Empaque"."Material_Empaque" "Material_Empaque"
	,"Material"."Material_Empaque_Id" "Material_Empaque_Id"
	,"Material"."Clase_Material_Id" "Clase_Material_Id"
	,"Clase_Material"."Clase_Material" "Clase_Material"
	,"Material"."Tipo_Material_Id" "Tipo_Material_Id"
	,"Tipo_Material"."Tipo_Material" "Tipo_Material"
from "APP"."Material" "Material"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Material"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Material"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Material"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."MRP_Type" "MRP_Type" on "Material"."MRP_Type_Id" = "MRP_Type"."MRP_Type_Id"
	left join "APP"."Clasificacion" "Clasificacion" on "Material"."Clasificacion_Id" = "Clasificacion"."Clasificacion_Id"
	left join "APP"."Registrado_Desde" "Registrado_Desde" on "Material"."Registrado_Desde_Id" = "Registrado_Desde"."Registrado_Desde_Id"
	left join "APP"."Material_Empaque" "Material_Empaque" on "Material"."Material_Empaque_Id" = "Material_Empaque"."Material_Empaque_Id"
	left join "APP"."Clase_Material" "Clase_Material" on "Material"."Clase_Material_Id" = "Clase_Material"."Clase_Material_Id"
	left join "APP"."Tipo_Material" "Tipo_Material" on "Material"."Tipo_Material_Id" = "Tipo_Material"."Tipo_Material_Id"
]]></text>
    </command>
