          ,{ name: 'Material_Producto', newValue: [Material_Producto] }
          ,{ name: 'Material_Orden_Produccion_Detalle', newValue: [Material_Orden_Produccion_Detalle] }
          ,{ name: 'Area_Entrega_Id', newValue: [Area_Entrega_Id] }
          ]
        ,done: function (result){
          eval(result.clientScript); 
          fnDebug("ExecCS01 result : ")
          fnDebug(result)                      	
          }                   
      })
	}]]></rule>
