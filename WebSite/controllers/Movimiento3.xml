  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 

SET @Already_Exists = 0 

SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   APP.Movimiento 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Movimiento), NULL)=dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Movimiento), NULL) 

IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](258,1,74,@Movimiento) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](258,1,74,@Movimiento) 
   RETURN 
END]]></rule>
