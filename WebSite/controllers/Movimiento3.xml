	left join "dbo"."Usuario_V" "Actualizado_Por" on "Movimiento"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Movimiento_Solicitado_Por" on "Movimiento"."Movimiento_Solicitado_Por_Id" = "Movimiento_Solicitado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Movimiento_Iniciado_Por" on "Movimiento"."Movimiento_Iniciado_Por_Id" = "Movimiento_Iniciado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Movimiento_Realizado_Por" on "Movimiento"."Movimiento_Realizado_Por_Id" = "Movimiento_Realizado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Movimiento_Cancelado_Por" on "Movimiento"."Movimiento_Cancelado_Por_Id" = "Movimiento_Cancelado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Movimiento_Capturado_Por" on "Movimiento"."Movimiento_Capturado_Por_Id" = "Movimiento_Capturado_Por"."Usuario_Sistema_Id"
	left join "APP"."Estatus_Movimiento" "Estatus_Devolucion" on "Movimiento"."Estatus_Devolucion_Id" = "Estatus_Devolucion"."Estatus_Movimiento_Id"
	left join "APP"."Estatus_Movimiento" "Estatus_Transferencia" on "Movimiento"."Estatus_Transferencia_Id" = "Estatus_Transferencia"."Estatus_Movimiento_Id"
	left join "APP"."Motivo_Movimiento" "Movimiento_Motivo_Cancelacion" on "Movimiento"."Movimiento_Motivo_Cancelacion_Id" = "Movimiento_Motivo_Cancelacion"."Motivo_Movimiento_Id"
]]></text>
