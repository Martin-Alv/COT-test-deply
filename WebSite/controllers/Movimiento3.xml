	,"Devolucion_Ubicacion_Destino"."Area_Entrega" "Devolucion_Ubicacion_Destino_Area_Entrega"
	,"Movimiento"."Estatus_Devolucion_Id" "Estatus_Devolucion_Id"
	,"Estatus_Devolucion"."Estatus_Movimiento" "Estatus_Devolucion"
	,"Movimiento"."Fecha_Devolucion" "Fecha_Devolucion"
	,"Movimiento"."Devolucion_Fecha_Cancelacion" "Devolucion_Fecha_Cancelacion"
	,"Movimiento"."Transferencia_Ubicacion_Origen_Id" "Transferencia_Ubicacion_Origen_Id"
	,"Transferencia_Ubicacion_Origen"."Ubicacion_Almacenamiento" "Transferencia_Ubicacion_Origen_Ubicacion_Almacenamiento"
	,"Movimiento"."Transferencia_Ubicacion_Destino_Id" "Transferencia_Ubicacion_Destino_Id"
	,"Transferencia_Ubicacion_Destino"."Ubicacion_Almacenamiento" "Transferencia_Ubicacion_Destino_Ubicacion_Almacenamiento"
	,"Movimiento"."Fecha_Transferencia" "Fecha_Transferencia"
