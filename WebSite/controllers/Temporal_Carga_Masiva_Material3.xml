    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT 
SET @Already_Exists = 0 
SELECT 
   TOP 1 @Already_Exists  = 1 
FROM 
   dbo.Temporal_Carga_Masiva_Material 
WHERE 
   dbo.fn_Sustituir_Caracteres_Especiales(Lower(Temporal_Carga_Masiva_Material_Id)) = dbo.fn_Sustituir_Caracteres_Especiales(Lower(@Temporal_Carga_Masiva_Material_Id)) -- Verificar si aplica campo
IF @Already_Exists = 1 
BEGIN 
   SET @BusinessRules_PreventDefault = 1 
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](1443,1,1,@Temporal_Carga_Masiva_Material_Id) 
   SET @Result_ShowAlert = [dbo].[fn_Obtener_Mensaje_Sistema](1443,1,1,@Temporal_Carga_Masiva_Material_Id) 
   SET @Estatus_Procesamiento_Id = 0
