  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Ubicacion_Almacenamiento_Area_Entrega
WHERE
	Ubicacion_Almacenamiento_Id = @Ubicacion_Almacenamiento_Id
AND Area_Entrega_Id = @Area_Entrega_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](323,1,87,@Ubicacion_Almacenamiento + ' - ' + @Area_Entrega)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Already_Exists BIT

SET @Already_Exists = 0

SELECT
   TOP 1 @Already_Exists  = 1
FROM
   APP.Ubicacion_Almacenamiento_Area_Entrega
WHERE
	Ubicacion_Almacenamiento_Id = @Ubicacion_Almacenamiento_Id
AND Area_Entrega_Id = @Area_Entrega_Id
AND Ubicacion_Almacenamiento_Area_Entrega_Id <> @Ubicacion_Almacenamiento_Area_Entrega_Id

IF @Already_Exists = 1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = [dbo].[fn_Obtener_Mensaje_Sistema](324,1,87,@Ubicacion_Almacenamiento + ' - ' + @Area_Entrega)
   SET @Result_ShowAlert = @Result_ShowViewMessage
   RETURN
END 
]]></rule>
