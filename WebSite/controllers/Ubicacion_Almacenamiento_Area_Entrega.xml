    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Ubicacion_Almacenamiento_Area_Entrega"."Ubicacion_Almacenamiento_Area_Entrega_Id" "Ubicacion_Almacenamiento_Area_Entrega_Id"
	,"Ubicacion_Almacenamiento_Area_Entrega"."Ubicacion_Almacenamiento_Id" "Ubicacion_Almacenamiento_Id"
	,"Ubicacion_Almacenamiento_"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento"
	,"Ubicacion_Almacenamiento_Area_Entrega"."Area_Entrega_Id" "Area_Entrega_Id"
	,"Area_Entrega_"."Area_Entrega" "Area_Entrega"
	,"Creado_Por"."Usuario_Sistema" "Creado_Por"
	,"Ubicacion_Almacenamiento_Area_Entrega"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."Usuario_Sistema" "Actualizado_Por"
	,"Ubicacion_Almacenamiento_Area_Entrega"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Ubicacion_Almacenamiento_Area_Entrega"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro"
from "APP"."Ubicacion_Almacenamiento_Area_Entrega" "Ubicacion_Almacenamiento_Area_Entrega"
	left join "APP"."Ubicacion_Almacenamiento" "Ubicacion_Almacenamiento_" on "Ubicacion_Almacenamiento_Area_Entrega"."Ubicacion_Almacenamiento_Id" = "Ubicacion_Almacenamiento_"."Ubicacion_Almacenamiento_Id"
	left join "APP"."Area_Entrega" "Area_Entrega_" on "Ubicacion_Almacenamiento_Area_Entrega"."Area_Entrega_Id" = "Area_Entrega_"."Area_Entrega_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Ubicacion_Almacenamiento_Area_Entrega"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Ubicacion_Almacenamiento_Area_Entrega"."Creado_Por_Id" = "Creado_Por"."Usuario_Sistema_Id"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Ubicacion_Almacenamiento_Area_Entrega"."Actualizado_Por_Id" = "Actualizado_Por"."Usuario_Sistema_Id"
]]></text>
    </command>
